msgid "E_D_LATERAL_RAISE_KETTLEBELL_EQUIP"
msgstr "Kettlebell"
msgid "E_D_LATERAL_RAISE_KETTLEBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_LATERAL_RAISE_KETTLEBELL_PREP_TWO"
