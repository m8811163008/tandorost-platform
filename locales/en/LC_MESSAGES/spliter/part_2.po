msgid "E_D_BENCH_PRESS_DUMBBELL_TITLE"
msgstr "Bench Press: Dumbbell"
msgid "E_D_BENCH_PRESS_DUMBBELL_FOCUS_AREA"
msgstr "Chest, Triceps, Shoulders"
msgid "E_D_BENCH_PRESS_DUMBBELL_EQUIPMENT"
msgstr "Dumbbell, Flat Bench"
msgid "E_D_BENCH_PRESS_DUMBBELL_PREP_ONE"
msgstr "Lie on a bench with feet on the floor."
msgid "E_D_BENCH_PRESS_DUMBBELL_PREP_TWO"
msgstr "Hold a dumbbell in each hand with elbows bent and palms facing forward on each side of your chest."
msgid "E_D_BENCH_PRESS_DUMBBELL_EXEC_ONE"
msgstr "Exhale and squeeze your chest to push the dumbbells up to extend your arms straight,"
msgid "E_D_BENCH_PRESS_DUMBBELL_EXEC_TWO"
msgstr "Inhale and slowly lower dumbbells to the starting position and repeat."
msgid "E_D_BENCH_PRESS_DUMBBELL_TIP_ONE"
msgstr "Do not lock out your elbows at the top."
msgid "E_D_BENCH_PRESS_DUMBBELL_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_BENCH_PRESS_SMITH_MACHINE_TITLE"
msgstr "Bench Press: Smith Machine"
msgid "E_D_BENCH_PRESS_SMITH_MACHINE_FOCUS_AREA"
msgstr "Chest, Shoulders, Triceps"
msgid "E_D_BENCH_PRESS_SMITH_MACHINE_EQUIPMENT"
msgstr "Smith Machine, Flat Bench"
msgid "E_D_BENCH_PRESS_SMITH_MACHINE_PREP_ONE"
msgstr "Lie on the bench under the machine with feet on the floor."
msgid "E_D_BENCH_PRESS_SMITH_MACHINE_PREP_TWO"
msgstr "Hold the bar over your chest in a shoulder width grip with arms bent and palms facing forward."
msgid "E_D_BENCH_PRESS_SMITH_MACHINE_EXEC_ONE"
msgstr "Exhale and squeeze your chest to push the bar up to extend your arms straight."
msgid "E_D_BENCH_PRESS_SMITH_MACHINE_EXEC_TWO"
msgstr "Inhale and slowly lower your bar to the starting position and repeat."
msgid "E_D_BENCH_PRESS_SMITH_MACHINE_TIP_ONE"
msgstr "Do not lock out your elbows at the top."
msgid "E_D_BENCH_PRESS_SMITH_MACHINE_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_BENCH_SQUAT_TITLE"
msgstr "Bench Squat"
msgid "E_D_BENCH_SQUAT_FOCUS_AREA"
msgstr "Quadriceps, Glutes"
msgid "E_D_BENCH_SQUAT_EQUIPMENT"
msgstr "Bodyweight"
msgid "E_D_BENCH_SQUAT_PREP_ONE"
msgstr "Place a bench behind you."
msgid "E_D_BENCH_SQUAT_PREP_TWO"
msgstr "Stand with feet shoulder width apart. Clasp your hands in front of your chest."
msgid "E_D_BENCH_SQUAT_EXEC_ONE"
msgstr "Squat down until your buttocks touch the bench."
msgid "E_D_BENCH_SQUAT_EXEC_TWO"
msgstr "Push through your heels and midfoot into the floor to the starting position slowly and repeat."
msgid "E_D_BENCH_SQUAT_TIP_ONE"
msgstr "Do not bounce on the bench to assist the movement."
msgid "E_D_BENCH_SQUAT_TIP_TWO"
msgstr "Do not lock out your knees at the top."
msgid "E_D_BENCH_SQUAT_TIP_THREE"
msgstr "Keep your eyes looking forward throughout the movement."
msgid "E_D_BENCH_SQUAT_TIP_FOUR"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_BENCH_SQUAT_TIP_FIVE"
msgstr "Keep your glutes, quadriceps and hamstrings engaged."
msgid "E_D_BENCH_SQUAT_BARBELL_TITLE"
msgstr "Bench Squat: Barbell"
msgid "E_D_BENCH_SQUAT_BARBELL_FOCUS_AREA"
msgstr "Quadriceps, Glutes"
msgid "E_D_BENCH_SQUAT_BARBELL_EQUIPMENT"
msgstr "Barbell, Flat Bench"
msgid "E_D_BENCH_SQUAT_BARBELL_PREP_ONE"
msgstr "Place a bench behind you and standing with feet. shoulder width apart."
msgid "E_D_BENCH_SQUAT_BARBELL_PREP_TWO"
msgstr "Hold the barbell on the back of your shoulders with palms facing forward."
msgid "E_D_BENCH_SQUAT_BARBELL_EXEC_ONE"
msgstr "Squat down until your buttocks touch the bench."
msgid "E_D_BENCH_SQUAT_BARBELL_EXEC_TWO"
msgstr "Push through your heels and midfoot into the floor to the starting position slowly and repeat."
msgid "E_D_BENCH_SQUAT_BARBELL_TIP_ONE"
msgstr "Do not bounce on the bench to assist the movement."
msgid "E_D_BENCH_SQUAT_BARBELL_TIP_TWO"
msgstr "Do not lock out your knees at the top."
msgid "E_D_BENCH_SQUAT_BARBELL_TIP_THREE"
msgstr "Keep your eyes looking forward throughout the movement."
msgid "E_D_BENCH_SQUAT_BARBELL_TIP_FOUR"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_BENCH_SQUAT_BARBELL_TIP_FIVE"
msgstr "Keep your glutes, quadriceps and hamstrings engaged."
msgid "E_D_BENCH_SQUAT_DUMBBELL_TITLE"
msgstr "Bench Squat- Dumbbell"
msgid "E_D_BENCH_SQUAT_DUMBBELL_FOCUS_AREA"
msgstr "Quadriceps, Glutes"
msgid "E_D_BENCH_SQUAT_DUMBBELL_EQUIPMENT"
msgstr "Dumbbell, Flat Bench"
msgid "E_D_BENCH_SQUAT_DUMBBELL_PREP_ONE"
msgstr "Place a bench behind you and standing with feet shoulder width apart."
msgid "E_D_BENCH_SQUAT_DUMBBELL_PREP_TWO"
msgstr "Hold a dumbbell in each hand in a neutral grip with arms extended at your sides."
msgid "E_D_BENCH_SQUAT_DUMBBELL_EXEC_ONE"
msgstr "Squat down until your buttocks touch the bench."
msgid "E_D_BENCH_SQUAT_DUMBBELL_EXEC_TWO"
msgstr "Push through your heels and midfoot into the floor to the starting position slowly and repeat."
msgid "E_D_BENCH_SQUAT_DUMBBELL_TIP_ONE"
msgstr "Do not bounce on the bench to assist the movement."
msgid "E_D_BENCH_SQUAT_DUMBBELL_TIP_TWO"
msgstr "Do not lock out your knees at the top."
msgid "E_D_BENCH_SQUAT_DUMBBELL_TIP_THREE"
msgstr "Keep your eyes looking forward throughout the movement."
msgid "E_D_BENCH_SQUAT_DUMBBELL_TIP_FOUR"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_BENCH_SQUAT_DUMBBELL_TIP_FIVE"
msgstr "Keep your glutes, quadriceps and hamstrings engaged"
msgid "E_D_BENT_ARM_PULLOVER_BARBELL_TITLE"
msgstr "Bent-Arm Pullover : Barbell"
msgid "E_D_BENT_ARM_PULLOVER_BARBELL_FOCUS_AREA"
msgstr "Back, Triceps, Chest"
msgid "E_D_BENT_ARM_PULLOVER_BARBELL_EQUIPMENT"
msgstr "Barbell, Flat Bench"
msgid "E_D_BENT_ARM_PULLOVER_BARBELL_PREP_ONE"
msgstr "Lie on a bench with feet on the floor."
msgid "E_D_BENT_ARM_PULLOVER_BARBELL_PREP_TWO"
msgstr "Hold the barbell using a shoulder width overhand grip and press it over your chest with arms stretched."
msgid "E_D_BENT_ARM_PULLOVER_BARBELL_EXEC_ONE"
msgstr "Inhale to bend your elbows and lower your arms in an arc behind your head"
msgid "E_D_BENT_ARM_PULLOVER_BARBELL_EXEC_TWO"
msgstr "Stretch out your chest and pause for a few seconds."
msgid "E_D_BENT_ARM_PULLOVER_BARBELL_EXEC_THREE"
msgstr "Exhale to pull the barbell in arc over your head until your arms are vertical to your body."
msgid "E_D_BENT_ARM_PULLOVER_BARBELL_EXEC_FOUR"
msgstr "Repeat slowly."
msgid "E_D_BENT_ARM_PULLOVER_BARBELL_TIP_ONE"
msgstr "Do not lock out your elbows at the top."
msgid "E_D_BENT_ARM_PULLOVER_BARBELL_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_BENT_KNEE_BICYCLE_CRUNCH_TITLE"
msgstr "Bent-Knee Bicycle Crunch"
msgid "E_D_BENT_KNEE_BICYCLE_CRUNCH_FOCUS_AREA"
msgstr "Abs, Quadriceps"
msgid "E_D_BENT_KNEE_BICYCLE_CRUNCH_EQUIPMENT"
msgstr "Bodyweight"
msgid "E_D_BENT_KNEE_BICYCLE_CRUNCH_PREP_ONE"
msgstr "Lie on the floor or a mat with knees bent."
msgid "E_D_BENT_KNEE_BICYCLE_CRUNCH_PREP_TWO"
msgstr "Place your hands behind your head with elbows wide."
msgid "E_D_BENT_KNEE_BICYCLE_CRUNCH_EXEC_ONE"
msgstr "Squeeze your abs and rotate your upper body to bring you knee to the opposite elbow."
msgid "E_D_BENT_KNEE_BICYCLE_CRUNCH_EXEC_TWO"
msgstr "Return to the starting position slowly and complete alternating between left and right,"
msgid "E_D_BENT_KNEE_BICYCLE_CRUNCH_TIP_ONE"
msgstr "Keep your head in a natural position and do not strain or pull on your neck."
msgid "E_D_BENT_KNEE_BICYCLE_CRUNCH_TIP_TWO"
msgstr "Lift your shoulder blades slightly off the floor."
msgid "E_D_BENT_KNEE_BICYCLE_CRUNCH_TIP_THREE"
msgstr "Keep a braced core during the exercise."
msgid "E_D_BENT_KNEE_SIDE_PLANK_TITLE"
msgstr "Bent-Knee Side Plank"
msgid "E_D_BENT_KNEE_SIDE_PLANK_FOCUS_AREA"
msgstr "Abs"
msgid "E_D_BENT_KNEE_SIDE_PLANK_EQUIPMENT"
msgstr "Bodyweight"
msgid "E_D_BENT_KNEE_SIDE_PLANK_PREP_ONE"
msgstr "Lie flat on one side and bend at the knees."
msgid "E_D_BENT_KNEE_SIDE_PLANK_PREP_TWO"
msgstr "Uplift you torso and thighs and support them with a stretched arm. Put the other hand on your waist."
msgid "E_D_BENT_KNEE_SIDE_PLANK_EXEC_ONE"
msgstr "Keep this position as long as you can."
msgid "E_D_BENT_KNEE_SIDE_PLANK_EXEC_TWO"
msgstr "Relax and switch to the other side and repeat the same repetitions."
msgid "E_D_BENT_KNEE_SIDE_PLANK_TIP_ONE"
msgstr "Keep your head, torso and thighs in a straight line."
msgid "E_D_BENT_KNEE_SIDE_PLANK_TIP_TWO"
msgstr "Keep a braced core during the exercise."
msgid "E_D_BENT_KNEE_SIDE_PLANK_TIP_THREE"
msgstr "Do not put the body weight on your shoulder."
msgid "E_D_BENT_KNEE_SIDE_PLANK_TIP_FOUR"
msgstr "Do not lock out your elbow during the exercise."
msgid "E_D_BENT_KNEE_WIPERS_TITLE"
msgstr "Bent-Knee Wipers"
msgid "E_D_BENT_KNEE_WIPERS_FOCUS_AREA"
msgstr "Abs"
msgid "E_D_BENT_KNEE_WIPERS_EQUIPMENT"
msgstr "Bodyweight"
msgid "E_D_BENT_KNEE_WIPERS_PREP_ONE"
msgstr "Lie on the floor with your arms stretched to your sides to stabilize your body."
msgid "E_D_BENT_KNEE_WIPERS_PREP_TWO"
msgstr "Lift your legs close together off the ground and bend your knees at 90 degrees."
msgid "E_D_BENT_KNEE_WIPERS_EXEC_ONE"
msgstr "Twist your legs to one side while keeping your upper body in place and your knees and feet together."
msgid "E_D_BENT_KNEE_WIPERS_EXEC_TWO"
msgstr "Pause for a few seconds and return to the starting position slowly."
msgid "E_D_BENT_KNEE_WIPERS_EXEC_THREE"
msgstr "Complete alternating between left and right."
msgid "E_D_BENT_KNEE_WIPERS_TIP_ONE"
msgstr "Do not let your legs bounce on the floor to assist the movement."
msgid "E_D_BENT_KNEE_WIPERS_TIP_TWO"
msgstr "To add difficulty, keep your legs straight."
msgid "E_D_BENT_KNEE_WIPERS_TIP_THREE"
msgstr "Keep a braced core during the exercise."
msgid "E_D_BENT_OVER_REAR_DELT_ROW_BARBELL_TITLE"
msgstr "Bent-over Rear Delt Row: Barbell"
msgid "E_D_BENT_OVER_REAR_DELT_ROW_BARBELL_FOCUS_AREA"
msgstr "Shoulders, Traps"
msgid "E_D_BENT_OVER_REAR_DELT_ROW_BARBELL_EQUIPMENT"
msgstr "Barbell"
msgid "E_D_BENT_OVER_REAR_DELT_ROW_BARBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_BENT_OVER_REAR_DELT_ROW_BARBELL_PREP_TWO"
msgstr "Bend at the hips and knees to hold the barbell near your chins in an overhand grip."
msgid "E_D_BENT_OVER_REAR_DELT_ROW_BARBELL_EXEC_ONE"
msgstr "Exhale and bend your elbows to lift the bar until the upper arms are parallel to the floor."
msgid "E_D_BENT_OVER_REAR_DELT_ROW_BARBELL_EXEC_TWO"
msgstr "Squeeze your shoulder blades at the top for a few seconds."
msgid "E_D_BENT_OVER_REAR_DELT_ROW_BARBELL_EXEC_THREE"
msgstr "Inhale and return to the starting position slowly and repeat."
msgid "E_D_BENT_OVER_REAR_DELT_ROW_BARBELL_TIP_ONE"
msgstr "Move the bar vertically."
msgid "E_D_BENT_OVER_REAR_DELT_ROW_BARBELL_TIP_TWO"
msgstr "Keep a straight back during the exercise,"
msgid "E_D_BENT_OVER_REAR_DELT_ROW_DUMBBELL_TITLE"
msgstr "Bent-over Rear Delt Row: Dumbbell"
msgid "E_D_BENT_OVER_REAR_DELT_ROW_DUMBBELL_FOCUS_AREA"
msgstr "Shoulders, Traps"
msgid "E_D_BENT_OVER_REAR_DELT_ROW_DUMBBELL_EQUIPMENT"
msgstr "Dumbbell, Flat Bench"
msgid "E_D_BENT_OVER_REAR_DELT_ROW_DUMBBELL_PREP_ONE"
msgstr "Put one leg and one arm of the same side on the bench to support you."
msgid "E_D_BENT_OVER_REAR_DELT_ROW_DUMBBELL_PREP_TWO"
msgstr "Bend over to hold the dumbbell in the free hand with your arm extended."
msgid "E_D_BENT_OVER_REAR_DELT_ROW_DUMBBELL_EXEC_ONE"
msgstr "Exhale and bend your elbow to lift the dumbbell to your side until the upper arm is parallel to the floor"
msgid "E_D_BENT_OVER_REAR_DELT_ROW_DUMBBELL_EXEC_TWO"
msgstr "Squeeze your shoulder blades at the top for a few seconds."
msgid "E_D_BENT_OVER_REAR_DELT_ROW_DUMBBELL_EXEC_THREE"
msgstr "Inhale and return to the starting position slowly and repeat."
msgid "E_D_BENT_OVER_REAR_DELT_ROW_DUMBBELL_EXEC_FOUR"
msgstr "Switch to the other side and repeat the same repetitions."
msgid "E_D_BENT_OVER_REAR_DELT_ROW_DUMBBELL_TIP_ONE"
msgstr "Move the dumbbell vertically."
msgid "E_D_BENT_OVER_REAR_DELT_ROW_DUMBBELL_TIP_TWO"
msgstr "Keep a straight back during the exercise"
msgid "E_D_BENT_OVER_REAR_DELT_ROW_KETTLEBELL_TITLE"
msgstr "Bent-over Rear Delt Row: Kettlebell"
msgid "E_D_BENT_OVER_REAR_DELT_ROW_KETTLEBELL_FOCUS_AREA"
msgstr "Shoulders, Traps"
msgid "E_D_BENT_OVER_REAR_DELT_ROW_KETTLEBELL_EQUIPMENT"
msgstr "Kettlebell, Flat Bench"
msgid "E_D_BENT_OVER_REAR_DELT_ROW_KETTLEBELL_PREP_ONE"
msgstr "Put one leg and one arm of the same side on the bench to support you."
msgid "E_D_BENT_OVER_REAR_DELT_ROW_KETTLEBELL_PREP_TWO"
msgstr "Bend over to hold the kettlebell from the floor in the free hand in an overhand grip"
msgid "E_D_BENT_OVER_REAR_DELT_ROW_KETTLEBELL_EXEC_ONE"
msgstr "Exhale and bend your elbow to lift the kettlebell to your side until the upper arm is parallel to the floor"
msgid "E_D_BENT_OVER_REAR_DELT_ROW_KETTLEBELL_EXEC_TWO"
msgstr "Squeeze your shoulder blades at the top for a few seconds."
msgid "E_D_BENT_OVER_REAR_DELT_ROW_KETTLEBELL_EXEC_THREE"
msgstr "Inhale and return to the starting position slowly and repeat."
msgid "E_D_BENT_OVER_REAR_DELT_ROW_KETTLEBELL_EXEC_FOUR"
msgstr "Switch to the other side and repeat the same repetitions."
msgid "E_D_BENT_OVER_REAR_DELT_ROW_KETTLEBELL_TIP_ONE"
msgstr "Move the kettlebell vertically."
msgid "E_D_BENT_OVER_REAR_DELT_ROW_KETTLEBELL_TIP_TWO"
msgstr "Keep a straight back during the exercise"
msgid "E_D_BENT_OVER_REAR_DELT_ROW_SMITH_MACHINE_TITLE"
msgstr "Bent-over Rear Delt Row: Smith Machine"
msgid "E_D_BENT_OVER_REAR_DELT_ROW_SMITH_MACHINE_FOCUS_AREA"
msgstr "Shoulders, Traps"
msgid "E_D_BENT_OVER_REAR_DELT_ROW_SMITH_MACHINE_EQUIPMENT"
msgstr "Smith Machine"
msgid "E_D_BENT_OVER_REAR_DELT_ROW_SMITH_MACHINE_PREP_ONE"
msgstr "Stand in the machine with feet shoulder width apart."
msgid "E_D_BENT_OVER_REAR_DELT_ROW_SMITH_MACHINE_PREP_TWO"
msgstr "Slightly bend at the hips and knees. Hold the bar next to your thighs in an overhand grip."
msgid "E_D_BENT_OVER_REAR_DELT_ROW_SMITH_MACHINE_EXEC_ONE"
msgstr "Exhale and bend your elbows to lift the bar as high as possible."
msgid "E_D_BENT_OVER_REAR_DELT_ROW_SMITH_MACHINE_EXEC_TWO"
msgstr "Squeeze your shoulder blades at the top for a few seconds. Inhale and return to the starting position slowly and repeat."
msgid "E_D_BENT_OVER_REAR_DELT_ROW_SMITH_MACHINE_TIP_ONE"
msgstr "Move the bar vertically."
msgid "E_D_BENT_OVER_REAR_DELT_ROW_SMITH_MACHINE_TIP_TWO"
msgstr "Keep a straight back during the exercise"
msgid "E_D_BENT_OVER_REVERSE_FLY_BAND_TITLE"
msgstr "Bent-over Reverse Fly: Band"
msgid "E_D_BENT_OVER_REVERSE_FLY_BAND_FOCUS_AREA"
msgstr "Shoulders, Traps"
msgid "E_D_BENT_OVER_REVERSE_FLY_BAND_EQUIPMENT"
msgstr "Handle Band"
msgid "E_D_BENT_OVER_REVERSE_FLY_BAND_PREP_ONE"
msgstr "Secure two bands to a low object and stand between them with feet shoulder width apart."
msgid "E_D_BENT_OVER_REVERSE_FLY_BAND_PREP_TWO"
msgstr "Bend over to hold the handles with the opposite hands. Extend your arms."
msgid "E_D_BENT_OVER_REVERSE_FLY_BAND_EXEC_ONE"
msgstr "Exhale and squeeze your shoulder blades together to raise your arms."
msgid "E_D_BENT_OVER_REVERSE_FLY_BAND_EXEC_TWO"
msgstr "Hold for a few seconds when your arms are parallel to your shoulders."
msgid "E_D_BENT_OVER_REVERSE_FLY_BAND_EXEC_THREE"
msgstr "Inhale and lower your arms to the starting position and repeat."
msgid "E_D_BENT_OVER_REVERSE_FLY_BAND_TIP_ONE"
msgstr "Keep your elbows and knees slightly bent throughout the movement."
msgid "E_D_BENT_OVER_REVERSE_FLY_BAND_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_BENT_OVER_REVERSE_FLY_DUMBBELL_TITLE"
msgstr "Bent-over Reverse Fly: Dumbbell"
msgid "E_D_BENT_OVER_REVERSE_FLY_DUMBBELL_FOCUS_AREA"
msgstr "Shoulders, Traps"
msgid "E_D_BENT_OVER_REVERSE_FLY_DUMBBELL_EQUIPMENT"
msgstr "Dumbbell"
msgid "E_D_BENT_OVER_REVERSE_FLY_DUMBBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_BENT_OVER_REVERSE_FLY_DUMBBELL_PREP_TWO"
msgstr "Bend over to hold a dumbbell in each hand with palms facing each other and arms extended downward."
msgid "E_D_BENT_OVER_REVERSE_FLY_DUMBBELL_EXEC_ONE"
msgstr "Exhale and squeeze your shoulder blades together to raise your arms."
msgid "E_D_BENT_OVER_REVERSE_FLY_DUMBBELL_EXEC_TWO"
msgstr "Hold for a few seconds when your arms are parallel to your shoulders."
msgid "E_D_BENT_OVER_REVERSE_FLY_DUMBBELL_EXEC_THREE"
msgstr "Inhale and lower your arms to the starting position and repeat."
msgid "E_D_BENT_OVER_REVERSE_FLY_DUMBBELL_TIP_ONE"
msgstr "Keep your elbows and knees slightly bent throughout the movement."
msgid "E_D_BENT_OVER_REVERSE_FLY_DUMBBELL_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_BENT_OVER_ROW_REVERSE_GRIP_BARBELL_TITLE"
msgstr "Bent-over Row (Reverse Grip): Barbell"
msgid "E_D_BENT_OVER_ROW_REVERSE_GRIP_BARBELL_FOCUS_AREA"
msgstr "Back, Traps, Biceps"
msgid "E_D_BENT_OVER_ROW_REVERSE_GRIP_BARBELL_EQUIPMENT"
msgstr "Barbell"
msgid "E_D_BENT_OVER_ROW_REVERSE_GRIP_BARBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart and bend over."
msgid "E_D_BENT_OVER_ROW_REVERSE_GRIP_BARBELL_PREP_TWO"
msgstr "Extend your arms and hold a barbell with an underhand grip at the middle of your shins."
msgid "E_D_BENT_OVER_ROW_REVERSE_GRIP_BARBELL_EXEC_ONE"
msgstr "Exhale and squeeze your lats muscle to lift the barbell up and arc shape towards your rib cage."
msgid "E_D_BENT_OVER_ROW_REVERSE_GRIP_BARBELL_EXEC_TWO"
msgstr "Pause for a few seconds, inhale, slowly lower the barbell to the starting position and repeat."
msgid "E_D_BENT_OVER_ROW_REVERSE_GRIP_BARBELL_TIP_ONE"
msgstr "Keep your knees slightly bent throughout the movement."
msgid "E_D_BENT_OVER_ROW_REVERSE_GRIP_BARBELL_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_BENT_OVER_ROW_REVERSE_GRIP_DUMBELL_TITLE"
msgstr "Bent-over Row (Reverse Grip): Dumbell"
msgid "E_D_BENT_OVER_ROW_REVERSE_GRIP_DUMBELL_FOCUS_AREA"
msgstr "Back, Traps, Biceps"
msgid "E_D_BENT_OVER_ROW_REVERSE_GRIP_DUMBELL_EQUIPMENT"
msgstr "Dumbbell"
msgid "E_D_BENT_OVER_ROW_REVERSE_GRIP_DUMBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart and bend over"
msgid "E_D_BENT_OVER_ROW_REVERSE_GRIP_DUMBELL_PREP_TWO"
msgstr "Hold a dumbbell in each hand with an underhand grip."
msgid "E_D_BENT_OVER_ROW_REVERSE_GRIP_DUMBELL_EXEC_ONE"
msgstr "Exhale and squeeze your lats muscle to lift the dumbbells up toward your rib cage."
msgid "E_D_BENT_OVER_ROW_REVERSE_GRIP_DUMBELL_EXEC_TWO"
msgstr "Pause for a few seconds, inhale, slowly lower the dumbbells to the starting position and repeat."
msgid "E_D_BENT_OVER_ROW_REVERSE_GRIP_DUMBELL_TIP_ONE"
msgstr "Keep your knees slightly bent throughout the movement."
msgid "E_D_BENT_OVER_ROW_REVERSE_GRIP_DUMBELL_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_BENT_OVER_ROW_WITH_TWIST_BAND_TITLE"
msgstr "Bent-over Row With Twist : Band"
msgid "E_D_BENT_OVER_ROW_WITH_TWIST_BAND_FOCUS_AREA"
msgstr "Back, Traps, Biceps"
msgid "E_D_BENT_OVER_ROW_WITH_TWIST_BAND_EQUIPMENT"
msgstr "Handle Band"
msgid "E_D_BENT_OVER_ROW_WITH_TWIST_BAND_PREP_ONE"
msgstr "Secure the band to a low object."
msgid "E_D_BENT_OVER_ROW_WITH_TWIST_BAND_PREP_TWO"
msgstr "Stand with feet shoulder width apart, bend your torso forward. Hold the handles shoulder width apart in a neutral grip and arms extended."
msgid "E_D_BENT_OVER_ROW_WITH_TWIST_BAND_EXEC_ONE"
msgstr "Exhale and squeeze your lats muscle to bring one elbow backward and extend your opposite arm forward."
msgid "E_D_BENT_OVER_ROW_WITH_TWIST_BAND_EXEC_TWO"
msgstr "Complete alternating between left and right."
msgid "E_D_BENT_OVER_ROW_WITH_TWIST_BAND_TIP_ONE"
msgstr "Keep your knees slightly bent throughout the movement."
msgid "E_D_BENT_OVER_ROW_WITH_TWIST_BAND_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_BENT_OVER_ROW_BAND_TITLE"
msgstr "Bent-over Row: Band"
msgid "E_D_BENT_OVER_ROW_BAND_FOCUS_AREA"
msgstr "Back, Traps, Biceps"
msgid "E_D_BENT_OVER_ROW_BAND_EQUIPMENT"
msgstr "Handle Band"
msgid "E_D_BENT_OVER_ROW_BAND_PREP_ONE"
msgstr "Stand with feet shoulder width apart and bend over"
msgid "E_D_BENT_OVER_ROW_BAND_PREP_TWO"
msgstr "Extend your arms and hold the ends of a band in bot hands."
msgid "E_D_BENT_OVER_ROW_BAND_PREP_THREE"
msgstr "Secure the band under midfoot."
msgid "E_D_BENT_OVER_ROW_BAND_EXEC_ONE"
msgstr "Exhale and squeeze your lats muscle to lift the band up toward your rib cage."
msgid "E_D_BENT_OVER_ROW_BAND_EXEC_TWO"
msgstr "Pause for a few seconds, inhale, slowly lower the band to the starting position and repeat."
msgid "E_D_BENT_OVER_ROW_BAND_TIP_ONE"
msgstr "Keep your upper arms steady by your sides."
msgid "E_D_BENT_OVER_ROW_BAND_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_BENT_OVER_ROW_BARBELL_TITLE"
msgstr "Bent-over Row: Barbell"
msgid "E_D_BENT_OVER_ROW_BARBELL_FOCUS_AREA"
msgstr "Back, Traps, Biceps"
msgid "E_D_BENT_OVER_ROW_BARBELL_EQUIPMENT"
msgstr "Barbell"
msgid "E_D_BENT_OVER_ROW_BARBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart and bend over."
msgid "E_D_BENT_OVER_ROW_BARBELL_PREP_TWO"
msgstr "Extend your arms and hold a barbell with an overhand grip at the middle of your shins."
msgid "E_D_BENT_OVER_ROW_BARBELL_EXEC_ONE"
msgstr "Exhale and squeeze your lats muscle to lift the barbell up towards your rib cage."
msgid "E_D_BENT_OVER_ROW_BARBELL_EXEC_TWO"
msgstr "Pause for a few seconds, inhale, slowly lower the barbell to the starting position and repeat."
msgid "E_D_BENT_OVER_ROW_BARBELL_TIP_ONE"
msgstr "Keep your knees slightly bent throughout the movement."
msgid "E_D_BENT_OVER_ROW_BARBELL_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_BENT_OVER_ROW_DUMBBELL_TITLE"
msgstr "Bent-over Row: Dumbbell"
msgid "E_D_BENT_OVER_ROW_DUMBBELL_FOCUS_AREA"
msgstr "Back, Traps, Biceps"
msgid "E_D_BENT_OVER_ROW_DUMBBELL_EQUIPMENT"
msgstr "Dumbbell"
msgid "E_D_BENT_OVER_ROW_DUMBBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart and bend over"
msgid "E_D_BENT_OVER_ROW_DUMBBELL_PREP_TWO"
msgstr "Hold a dumbbell in each hand with a neutral grip."
msgid "E_D_BENT_OVER_ROW_DUMBBELL_EXEC_ONE"
msgstr "Exhale and squeeze your lats muscle to lift the dumbbells up toward your rib cage."
msgid "E_D_BENT_OVER_ROW_DUMBBELL_EXEC_TWO"
msgstr "Pause for a few seconds, inhale, slowly lower the dumbbells to the starting position and repeat."
msgid "E_D_BENT_OVER_ROW_DUMBBELL_TIP_ONE"
msgstr "Keep your knees slightly bent throughout the movement."
msgid "E_D_BENT_OVER_ROW_DUMBBELL_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_BENT_OVER_ROW_KETTLEBELL_TITLE"
msgstr "Bent-over Row: Kettlebell"
msgid "E_D_BENT_OVER_ROW_KETTLEBELL_FOCUS_AREA"
msgstr "Back, Traps, Biceps"
msgid "E_D_BENT_OVER_ROW_KETTLEBELL_EQUIPMENT"
msgstr "Kettlebell"
msgid "E_D_BENT_OVER_ROW_KETTLEBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart and bend over"
msgid "E_D_BENT_OVER_ROW_KETTLEBELL_PREP_TWO"
msgstr "Hold a kettlebell in each hand with a neutral grip."
msgid "E_D_BENT_OVER_ROW_KETTLEBELL_EXEC_ONE"
msgstr "Exhale and squeeze your lats muscle to lift the kettlebells up toward your rib cage."
msgid "E_D_BENT_OVER_ROW_KETTLEBELL_EXEC_TWO"
msgstr "Pause for a few seconds, inhale, return to the starting position slowly and repeat."
msgid "E_D_BENT_OVER_ROW_KETTLEBELL_TIP_ONE"
msgstr "Keep your knees slightly bent throughout the movement."
msgid "E_D_BENT_OVER_ROW_KETTLEBELL_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_BENT_OVER_ROW_SMITH_MACHINE_TITLE"
msgstr "Bent-over Row: Smith Machine"
msgid "E_D_BENT_OVER_ROW_SMITH_MACHINE_FOCUS_AREA"
msgstr "Back, Traps, Biceps, Shoulders"
msgid "E_D_BENT_OVER_ROW_SMITH_MACHINE_EQUIPMENT"
msgstr "Smith Machine"
msgid "E_D_BENT_OVER_ROW_SMITH_MACHINE_PREP_ONE"
msgstr "Stand before the machine with your feet shoulder width apart and bend over."
msgid "E_D_BENT_OVER_ROW_SMITH_MACHINE_PREP_TWO"
msgstr "Hold the bar by both hands with an overhand grip."
msgid "E_D_BENT_OVER_ROW_SMITH_MACHINE_EXEC_ONE"
msgstr "Exhale and squeeze your lats muscle to lift the bar toward your rib cage."
msgid "E_D_BENT_OVER_ROW_SMITH_MACHINE_EXEC_TWO"
msgstr "Pause for a few seconds, inhale, lower the bar to the starting point slowly and repeat."
msgid "E_D_BENT_OVER_ROW_SMITH_MACHINE_TIP_ONE"
msgstr "Keep your knees slightly bent throughout the movement."
msgid "E_D_BENT_OVER_ROW_SMITH_MACHINE_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_BICEP_CURL_TITLE"
msgstr "Bicep Curl"
msgid "E_D_BICEP_CURL_PREP_ONE"
msgstr "Stand closely to an object with feet together."
msgid "E_D_BICEP_CURL_PREP_TWO"
msgstr "Hold it and extend your arms to lean back with the arms tucked to your sides."
msgid "E_D_BICEP_CURL_EXEC_ONE"
msgstr "Bend your arm to stand up straight."
msgid "E_D_BICEP_CURL_EXEC_TWO"
msgstr "Lean back to the starting position slowly and repeat."
msgid "E_D_BICEP_CURL_TIP_ONE"
msgstr "Keep your chest up and eyes looking forward throughout the movement."
msgid "E_D_BICEP_CURL_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_BICEP_CURL_BAND_TITLE"
msgstr "Bicep Curl: Band"
msgid "E_D_BICEP_CURL_BAND_PREP_ONE"
msgstr "Stand with one foot forward and place the band under the midfoot."
msgid "E_D_BICEP_CURL_BAND_PREP_TWO"
msgstr "Hold a handle in each hand in an underhand grip with elbows tucked to your sides."
msgid "E_D_BICEP_CURL_BAND_EXEC_ONE"
msgstr "Curl the band up until your forearms touch your chest."
msgid "E_D_BICEP_CURL_BAND_EXEC_TWO"
msgstr "Pause for a few seconds and squeeze your biceps."
msgid "E_D_BICEP_CURL_BAND_EXEC_THREE"
msgstr "Lower the band to the starting position and repeat."
msgid "E_D_BICEP_CURL_BAND_TIP_ONE"
msgstr "Keep your chest up and eyes looking forward throughout the movement."
msgid "E_D_BICEP_CURL_BAND_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_BICEP_CURL_BARBELL_TITLE"
msgstr "Bicep Curl: Barbell"
msgid "E_D_BICEP_CURL_BARBELL_PREP_ONE"
msgstr "Stand with your chest up."
msgid "E_D_BICEP_CURL_BARBELL_PREP_TWO"
msgstr "Hold the barbell in an underhand grip with hands shoulder width apart and elbows tucked to your sides."
msgid "E_D_BICEP_CURL_BARBELL_EXEC_ONE"
msgstr "Curl the bar up until your upper arms touch your chest."
msgid "E_D_BICEP_CURL_BARBELL_EXEC_TWO"
msgstr "Pause for a few seconds and squeeze your biceps."
msgid "E_D_BICEP_CURL_BARBELL_EXEC_THREE"
msgstr "Lower the bar to the starting position and repeat."
msgid "E_D_BICEP_CURL_BARBELL_TIP_ONE"
msgstr "Keep your chest up and eyes looking forward throughout the movement."
msgid "E_D_BICEP_CURL_BARBELL_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_BICEP_CURL_CABLE_TITLE"
msgstr "Bicep Curl: Cable"
msgid "E_D_BICEP_CURL_CABLE_PREP_ONE"
msgstr "Stand and hold the bar connected to the low pulley in an underhand grip and shoulder width apart with elbows tucked to your sides."
msgid "E_D_BICEP_CURL_CABLE_EXEC_ONE"
msgstr "Curl the bar up until your forearms touch your chest."
msgid "E_D_BICEP_CURL_CABLE_EXEC_TWO"
msgstr "Pause for a few seconds and squeeze your biceps."
msgid "E_D_BICEP_CURL_CABLE_EXEC_THREE"
msgstr "Lower the bar to the starting position and repeat."
msgid "E_D_BICEP_CURL_CABLE_TIP_ONE"
msgstr "Do not bounce back."
msgid "E_D_BICEP_CURL_CABLE_TIP_TWO"
msgstr "Keep your chest up and eyes looking forward throughout the movement."
msgid "E_D_BICEP_CURL_CABLE_TIP_THREE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_BICEP_CURL_DUMBBELL_TITLE"
msgstr "Bicep Curl: Dumbbell"
msgid "E_D_BICEP_CURL_DUMBBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart, Hold a dumbbell in each hand in an underhand grip with arms extended at your sides and elbows tucked to your sides."
msgid "E_D_BICEP_CURL_DUMBBELL_EXEC_ONE"
msgstr "Curl the dumbbells up until your forearms touch your chest."
msgid "E_D_BICEP_CURL_DUMBBELL_EXEC_TWO"
msgstr "Pause for a few seconds and squeeze your biceps."
msgid "E_D_BICEP_CURL_DUMBBELL_EXEC_THREE"
msgstr "Lower the dumbbells to the starting position and repeat."
msgid "E_D_BICEP_CURL_DUMBBELL_TIP_ONE"
msgstr "Keep your chest up and eyes looking forward throughout the movement."
msgid "E_D_BICEP_CURL_DUMBBELL_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_BICEP_CURL_MACHINE_TITLE"
msgstr "Bicep Curl: Machine"
msgid "E_D_BICEP_CURL_MACHINE_PREP_ONE"
msgstr "Sit on the machine with feet on the floor."
msgid "E_D_BICEP_CURL_MACHINE_PREP_TWO"
msgstr "Hold the handles at your sides in an underhand grip."
msgid "E_D_BICEP_CURL_MACHINE_EXEC_ONE"
msgstr "Curl one handle up until your forearm touches your chest."
msgid "E_D_BICEP_CURL_MACHINE_EXEC_TWO"
msgstr "Pause for a few seconds and squeeze your biceps"
msgid "E_D_BICEP_CURL_MACHINE_EXEC_THREE"
msgstr "Lower the handle to the starting position and repeat."
msgid "E_D_BICEP_CURL_MACHINE_EXEC_FOUR"
msgstr "Switch to the other side and repeat the same repetitions."
msgid "E_D_BICEP_CURL_MACHINE_TIP_ONE"
msgstr "Keep your chest up and eyes looking forward throughout the movement."
msgid "E_D_BICEP_CURL_MACHINE_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
