msgid "E_D_RENEGADE_ROW_KETTLEBELL_TIP_THREE"
msgstr "Do not et your chest touch the floor."
msgid "E_D_REVERSE_CONCENTRATION_CURL_DUMBBELL_TITLE"
msgstr "Reverse Concentration Curl: Dumbbell"
msgid "E_D_REVERSE_CONCENTRATION_CURL_DUMBBELL_FOCUS"
msgstr "Biceps, Forearms"
msgid "E_D_REVERSE_CONCENTRATION_CURL_DUMBBELL_EQUIP"
msgstr "Dumbbell, Flat Bench"
msgid "E_D_REVERSE_CONCENTRATION_CURL_DUMBBELL_PREP_ONE"
msgstr "Sit on a bench with feet on the floor."
msgid "E_D_REVERSE_CONCENTRATION_CURL_DUMBBELL_PREP_TWO"
msgstr "Hold a dumbbell in one hand using an underhand grip and put the elbow on your thigh on the same side."
msgid "E_D_REVERSE_CONCENTRATION_CURL_DUMBBELL_EXEC_ONE"
msgstr "Exhale and curl the dumbbell up to your shoulders."
msgid "E_D_REVERSE_CONCENTRATION_CURL_DUMBBELL_EXEC_TWO"
msgstr "Pause for a few seconds and squeeze your biceps when the dumbbell reaches the shoulder level."
msgid "E_D_REVERSE_CONCENTRATION_CURL_DUMBBELL_EXEC_THREE"
msgstr "Inhale and lower the dumbbell to the starting position and respect."
msgid "E_D_REVERSE_CONCENTRATION_CURL_DUMBBELL_EXEC_FOUR"
msgstr "Switch to the other side and repeat the same repetitions."
msgid "E_D_REVERSE_CONCENTRATION_CURL_DUMBBELL_TIP_ONE"
msgstr "Keep your wrists straight throughout the movement."
msgid "E_D_REVERSE_CONCENTRATION_CURL_DUMBBELL_TIP_TWO"
msgstr "Do not swing the dumbbell down."
msgid "E_D_REVERSE_CONCENTRATION_CURL_DUMBBELL_TIP_THREE"
msgstr "Keep your body stationary by using the free hand on the opposite thigh to support you"
msgid "E_D_REVERSE_CONCENTRATION_CURL_DUMBBELL_TIP_FOUR"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_REVERSE_CRUNCH_TITLE"
msgstr "Reverse Crunch"
msgid "E_D_REVERSE_CRUNCH_FOCUS"
msgstr "Abs"
msgid "E_D_REVERSE_CRUNCH_EQUIP"
msgstr "Bodyweight"
msgid "E_D_REVERSE_CRUNCH_PREP_ONE"
msgstr "Lie on your back and elevate your legs with knees bent at 90 degrees and thighs vertical to the floor."
msgid "E_D_REVERSE_CRUNCH_PREP_TWO"
msgstr "Position your arms at your sides to stabilize your trunk."
msgid "E_D_REVERSE_CRUNCH_EXEC_ONE"
msgstr "Squeeze your abs and roll back to bring your legs together towards your torso with hips off the ground."
msgid "E_D_REVERSE_CRUNCH_EXEC_TWO"
msgstr "Pause at the top for a few seconds, return to the starting position slowly and repeat."
msgid "E_D_REVERSE_CRUNCH_TIP_ONE"
msgstr "Keep your knees bent at 90 degrees throughout the movement."
msgid "E_D_REVERSE_CRUNCH_TIP_TWO"
msgstr "Keep a braced core during the exercise."
msgid "E_D_REVERSE_CRUNCH_CABLE_TITLE"
msgstr "Reverse Crunch: Cable"
msgid "E_D_REVERSE_CRUNCH_CABLE_FOCUS"
msgstr "Abs"
msgid "E_D_REVERSE_CRUNCH_CABLE_EQUIP"
msgstr "Cable Machine"
msgid "E_D_REVERSE_CRUNCH_CABLE_PREP_ONE"
msgstr "Secure your feet together with the attachment."
msgid "E_D_REVERSE_CRUNCH_CABLE_PREP_TWO"
msgstr "Lie down with arms extended at your sides and elevate your legs together with knees bent at 90 degrees."
msgid "E_D_REVERSE_CRUNCH_CABLE_EXEC_ONE"
msgstr "Squeeze your abs and roll back to bring your legs together towards your torso with hips off the ground."
msgid "E_D_REVERSE_CRUNCH_CABLE_EXEC_TWO"
msgstr "Pause at the top for a few seconds, return to the starting position slowly and repeat."
msgid "E_D_REVERSE_CRUNCH_CABLE_TIP_ONE"
msgstr "Keep your knees bent at 90 degrees throughout the movement."
msgid "E_D_REVERSE_CRUNCH_CABLE_TIP_TWO"
msgstr "Keep a braced core during the exercise."
msgid "E_D_REVERSE_CRUNCH_MACHINE_TITLE"
msgstr "Reverse Crunch: Machine"
msgid "E_D_REVERSE_CRUNCH_MACHINE_FOCUS"
msgstr "Abs"
msgid "E_D_REVERSE_CRUNCH_MACHINE_EQUIP"
msgstr "Crunch Machine"
msgid "E_D_REVERSE_CRUNCH_MACHINE_PREP_ONE"
msgstr "Lie on the machine with your thighs against the bar and secure your legs together with knees bent."
msgid "E_D_REVERSE_CRUNCH_MACHINE_PREP_TWO"
msgstr "Hold the handles overhead."
msgid "E_D_REVERSE_CRUNCH_MACHINE_EXEC_ONE"
msgstr "Exhale and squeeze your abs to bring the bar, towards your torso as much as possible."
msgid "E_D_REVERSE_CRUNCH_MACHINE_EXEC_TWO"
msgstr "Pause at the top for a few seconds, inhale and return to the starting position slowly and repeat."
msgid "E_D_REVERSE_CRUNCH_MACHINE_TIP_ONE"
msgstr "Keep your knees bent at 90 degrees throughout the movement."
msgid "E_D_REVERSE_CRUNCH_MACHINE_TIP_TWO"
msgstr "Keep a braced core during the exercise."
msgid "E_D_REVERSE_CURL_BAND_TITLE"
msgstr "Reverse Curl: Band"
msgid "E_D_REVERSE_CURL_BAND_FOCUS"
msgstr "Biceps, Forearms"
msgid "E_D_REVERSE_CURL_BAND_EQUIP"
msgstr "Handle Band"
msgid "E_D_REVERSE_CURL_BAND_PREP_ONE"
msgstr "Secure one band under your midfoot with feet shoulder width apart."
msgid "E_D_REVERSE_CURL_BAND_PREP_TWO"
msgstr "Hold the handles in an overhand grip with arms extended and hands next to your thighs."
msgid "E_D_REVERSE_CURL_BAND_EXEC_ONE"
msgstr "Exhale and curl the handles up to your shoulders bending at the elbows."
msgid "E_D_REVERSE_CURL_BAND_EXEC_TWO"
msgstr "Pause for a few seconds and squeeze your biceps when the handles reach the shoulder level."
msgid "E_D_REVERSE_CURL_BAND_EXEC_THREE"
msgstr "Inhale and lower the handles back to the starting position and respect."
msgid "E_D_REVERSE_CURL_BAND_TIP_ONE"
msgstr "Keep your wrists straight throughout the movement."
msgid "E_D_REVERSE_CURL_BAND_TIP_TWO"
msgstr "Do not swing the handles down."
msgid "E_D_REVERSE_CURL_BAND_TIP_THREE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_REVERSE_CURL_BARBELL_TITLE"
msgstr "Reverse Curl: Barbell"
msgid "E_D_REVERSE_CURL_BARBELL_FOCUS"
msgstr "Biceps, Forearms"
msgid "E_D_REVERSE_CURL_BARBELL_EQUIP"
msgstr "Barbell"
msgid "E_D_REVERSE_CURL_BARBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_REVERSE_CURL_BARBELL_PREP_TWO"
msgstr "Hold a barbell at hip level in an overhand grip with arms extended and tucked to your sides."
msgid "E_D_REVERSE_CURL_BARBELL_EXEC_ONE"
msgstr "Exhale and curl the barbell up to your shoulders bending at the elbows."
msgid "E_D_REVERSE_CURL_BARBELL_EXEC_TWO"
msgstr "Do not swing the dumbbell down"
msgid "E_D_REVERSE_CURL_BARBELL_EXEC_THREE"
msgstr "Inhale and lower the barbell back to the starting position and repeat."
msgid "E_D_REVERSE_CURL_BARBELL_TIP_ONE"
msgstr "Keep your wrists straight throughout the movement."
msgid "E_D_REVERSE_CURL_BARBELL_TIP_TWO"
msgstr "Do not swing the bar down."
msgid "E_D_REVERSE_CURL_BARBELL_TIP_THREE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_REVERSE_CURL_CABLE_TITLE"
msgstr "Reverse Curl: Cable"
msgid "E_D_REVERSE_CURL_CABLE_FOCUS"
msgstr "Biceps, Forearms"
msgid "E_D_REVERSE_CURL_CABLE_EQUIP"
msgstr "Cable Machine"
msgid "E_D_REVERSE_CURL_CABLE_PREP_ONE"
msgstr "Stand before the machine with feet shoulder width apart."
msgid "E_D_REVERSE_CURL_CABLE_PREP_TWO"
msgstr "Hold the bar at hip level in an overhand grip with arms extended and tucked to your sides."
msgid "E_D_REVERSE_CURL_CABLE_EXEC_ONE"
msgstr "Exhale and curl the bar up to your shoulders bending at the elbows."
msgid "E_D_REVERSE_CURL_CABLE_EXEC_TWO"
msgstr "Pause for a few seconds and squeeze your biceps when the bar reaches the shoulder level."
msgid "E_D_REVERSE_CURL_CABLE_EXEC_THREE"
msgstr "Inhale and lower the bar back to the starting position and repeat."
msgid "E_D_REVERSE_CURL_CABLE_TIP_ONE"
msgstr "Keep your wrists straight throughout the movement."
msgid "E_D_REVERSE_CURL_CABLE_TIP_TWO"
msgstr "Do not swing the bar down."
msgid "E_D_REVERSE_CURL_CABLE_TIP_THREE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_REVERSE_CURL_DUMBBELL_TITLE"
msgstr "Reverse Curl: Dumbbell"
msgid "E_D_REVERSE_CURL_DUMBBELL_FOCUS"
msgstr "Biceps, Forearms"
msgid "E_D_REVERSE_CURL_DUMBBELL_EQUIP"
msgstr "Dumbbell"
msgid "E_D_REVERSE_CURL_DUMBBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_REVERSE_CURL_DUMBBELL_PREP_TWO"
msgstr "Hold a dumbbell in each hand at hip level in an overhand grip with arms extended and tucked to your sides."
msgid "E_D_REVERSE_CURL_DUMBBELL_EXEC_ONE"
msgstr "Exhale and curl the dumbbells up to your shoulders bending at the elbows."
msgid "E_D_REVERSE_CURL_DUMBBELL_EXEC_TWO"
msgstr "Pause for a few seconds and squeeze your biceps when the dumbbells reach the shoulder level."
msgid "E_D_REVERSE_CURL_DUMBBELL_EXEC_THREE"
msgstr "Inhale and lower the dumbbells back to the starting position and repeat."
msgid "E_D_REVERSE_CURL_DUMBBELL_TIP_ONE"
msgstr "Keep your wrists straight throughout the movement."
msgid "E_D_REVERSE_CURL_DUMBBELL_TIP_TWO"
msgstr "Do not swing the dumbbell down"
msgid "E_D_REVERSE_CURL_DUMBBELL_TIP_THREE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_REVERSE_PLANK_TITLE"
msgstr "Reverse Plank"
msgid "E_D_REVERSE_PLANK_FOCUS"
msgstr "Abs, Shoulders, Quadriceps"
msgid "E_D_REVERSE_PLANK_EQUIP"
msgstr "Bodyweight"
msgid "E_D_REVERSE_PLANK_PREP_ONE"
msgstr "Sit on the floor with legs extended together."
msgid "E_D_REVERSE_PLANK_PREP_TWO"
msgstr "Place your hands behind you directly under your shoulders with your fingers pointing forward."
msgid "E_D_REVERSE_PLANK_EXEC_ONE"
msgstr "Press onto your palms and lift your hips and torso off the floor."
msgid "E_D_REVERSE_PLANK_EXEC_TWO"
msgstr "Squeeze your core and hold for as long as you can."
msgid "E_D_REVERSE_PLANK_TIP_ONE"
msgstr "Keep your head, torso and thighs in a straight line."
msgid "E_D_REVERSE_PLANK_TIP_TWO"
msgstr "Keep a braced core during the exercise."
msgid "E_D_REVERSE_PLANK_TIP_THREE"
msgstr "Keep your shoulders down and do not put the body weight on your shoulders."
msgid "E_D_REVERSE_PLANK_TIP_FOUR"
msgstr "Do not lock out your elbows during the exercise."
msgid "E_D_REVERSE_WRIST_CURL_BAND_TITLE"
msgstr "Reverse Wrist Curl: Band"
msgid "E_D_REVERSE_WRIST_CURL_BAND_FOCUS"
msgstr "Forearms"
msgid "E_D_REVERSE_WRIST_CURL_BAND_EQUIP"
msgstr "Handle Band, Flat Bench"
msgid "E_D_REVERSE_WRIST_CURL_BAND_PREP_ONE"
msgstr "Sit down and secure the band under one foot."
msgid "E_D_REVERSE_WRIST_CURL_BAND_PREP_TWO"
msgstr "Rest one forearm on your thigh with the wrist ahead of the knee."
msgid "E_D_REVERSE_WRIST_CURL_BAND_PREP_THREE"
msgstr "Hold the handle with an overhand grip."
msgid "E_D_REVERSE_WRIST_CURL_BAND_EXEC_ONE"
msgstr "Extend the curled wrist and pull the handle upwards until you eel a stretch in the working arm."
msgid "E_D_REVERSE_WRIST_CURL_BAND_EXEC_TWO"
msgstr "Return to the starting position and repeat."
msgid "E_D_REVERSE_WRIST_CURL_BAND_TIP_ONE"
msgstr "Use the opposite hand to secure the exercised forearm."
msgid "E_D_REVERSE_WRIST_CURL_BAND_TIP_TWO"
msgstr "Keep your thighs and the exercised forearm parallel to the floor."
msgid "E_D_REVERSE_WRIST_CURL_BAND_TIP_THREE"
msgstr "Keep your working elbow and wrist at the same. height during the exercise."
