msgid "E_D_RACK_PULL_BARBELL_TITLE"
msgstr "Rack Pull: Barbell"
msgid "E_D_RACK_PULL_BARBELL_FOCUS"
msgstr "Lower back, Glutes, Hamstrings, Quadriceps"
msgid "E_D_RACK_PULL_BARBELL_EQUIP"
msgstr "Barbell"
