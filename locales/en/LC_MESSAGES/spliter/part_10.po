msgid "E_D_OVERHEAD_PRESS_CLOSE_GRIP_BARBELL_TITLE"
msgstr "Overhead Press (Close Grip) Barbell"
msgid "E_D_OVERHEAD_PRESS_CLOSE_GRIP_BARBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_OVERHEAD_PRESS_CLOSE_GRIP_BARBELL_PREP_TWO"
msgstr "Hold the barbell in front of your shoulders in a narrow overhand grip with elbows pointing slightly forward."
msgid "E_D_OVERHEAD_PRESS_CLOSE_GRIP_BARBELL_EXEC_ONE"
msgstr "Exhale and squeeze your shoulders to lift the bar vertically until your arms are straight overhead."
msgid "E_D_OVERHEAD_PRESS_CLOSE_GRIP_BARBELL_EXEC_TWO"
msgstr "Pause for a few seconds, inhale, return to the starting position slowly and repeat."
msgid "E_D_OVERHEAD_PRESS_CLOSE_GRIP_BARBELL_TIP_ONE"
msgstr "Do not lock out your elbows at the top."
msgid "E_D_OVERHEAD_PRESS_CLOSE_GRIP_BARBELL_TIP_TWO"
msgstr "Do not rest the barbell on your shoulders."
msgid "E_D_OVERHEAD_PRESS_CLOSE_GRIP_BARBELL_TIP_THREE"
msgstr "Keep your knees slightly bent throughout the movement."
msgid "E_D_OVERHEAD_PRESS_CLOSE_GRIP_BARBELL_TIP_FOUR"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_OVERHEAD_PRESS_WIDE_GRIP_BARBELL_TITLE"
msgstr "Overhead Press (Wide Grip) Barbell"
msgid "E_D_OVERHEAD_PRESS_WIDE_GRIP_BARBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_OVERHEAD_PRESS_WIDE_GRIP_BARBELL_PREP_TWO"
msgstr "Hold the barbell in front of your shoulders in a wide overhand grip."
msgid "E_D_OVERHEAD_PRESS_WIDE_GRIP_BARBELL_EXEC_ONE"
msgstr "Exhale and squeeze your shoulders to lift the bar vertically until your arms are straight overhead."
msgid "E_D_OVERHEAD_PRESS_WIDE_GRIP_BARBELL_EXEC_TWO"
msgstr "Pause for a few seconds, inhale, return to the starting position slowly and repeat."
msgid "E_D_OVERHEAD_PRESS_WIDE_GRIP_BARBELL_TIP_ONE"
msgstr "Do not lock out your elbows at the top."
msgid "E_D_OVERHEAD_PRESS_WIDE_GRIP_BARBELL_TIP_TWO"
msgstr "Keep your knees slightly bent throughout the movement."
msgid "E_D_OVERHEAD_PRESS_WIDE_GRIP_BARBELL_TIP_THREE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_OVERHEAD_PRESS_BAND_TITLE"
msgstr "Overhead Press: Band"
msgid "E_D_OVERHEAD_PRESS_BAND_PREP_ONE"
msgstr "Stand with one foot on the middle of the band ahead of the other."
msgid "E_D_OVERHEAD_PRESS_BAND_PREP_TWO"
msgstr "Hold the handles with arms bent at 90 degrees at your sides and palms facing forward."
msgid "E_D_OVERHEAD_PRESS_BAND_EXEC_ONE"
msgstr "Exhale and squeeze your shoulders to press the handles vertically until your arms are straight overhead."
msgid "E_D_OVERHEAD_PRESS_BAND_EXEC_TWO"
msgstr "Pause for a few seconds, inhale, return to the starting position slowly and repeat."
msgid "E_D_OVERHEAD_PRESS_BAND_TIP_ONE"
msgstr "Do not lock out your elbows at the top."
msgid "E_D_OVERHEAD_PRESS_BAND_TIP_TWO"
msgstr "Lean slightly forward on your front leg during the exercise."
msgid "E_D_OVERHEAD_PRESS_BAND_TIP_THREE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_OVERHEAD_PRESS_BARBELL_TITLE"
msgstr "Overhead Press: Barbell"
msgid "E_D_OVERHEAD_PRESS_BARBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_OVERHEAD_PRESS_BARBELL_PREP_TWO"
msgstr "Hold the barbell in front of your shoulders with hands slightly wider than shoulder width apart and palms facing forward."
msgid "E_D_OVERHEAD_PRESS_BARBELL_EXEC_ONE"
msgstr "Exhale and squeeze your shoulders to lift the bar vertically until your arms are straight overhead."
msgid "E_D_OVERHEAD_PRESS_BARBELL_EXEC_TWO"
msgstr "Pause for a few seconds, inhale, return to the starting position slowly and repeat."
msgid "E_D_OVERHEAD_PRESS_BARBELL_TIP_ONE"
msgstr "Do not lock out your elbows at the top."
msgid "E_D_OVERHEAD_PRESS_BARBELL_TIP_TWO"
msgstr "Do not rest the barbell on your shoulders."
msgid "E_D_OVERHEAD_PRESS_BARBELL_TIP_THREE"
msgstr "Keep your knees slightly bent throughout the movement."
msgid "E_D_OVERHEAD_PRESS_BARBELL_TIP_FOUR"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_OVERHEAD_PRESS_CABLE_TITLE"
msgstr "Overhead Press: Cable"
msgid "E_D_OVERHEAD_PRESS_CABLE_PREP_ONE"
msgstr "Stand back to the machine with feet shoulder width apart."
msgid "E_D_OVERHEAD_PRESS_CABLE_PREP_TWO"
msgstr "Hold the handles at your sides with your elbows bent at 90 degrees and palms facing forward."
msgid "E_D_OVERHEAD_PRESS_CABLE_EXEC_ONE"
msgstr "Exhale and squeeze your shoulders to lift the handles vertically until your arms are straight overhead."
msgid "E_D_OVERHEAD_PRESS_CABLE_EXEC_TWO"
msgstr "Pause for a few seconds, inhale, return to the starting position slowly and repeat."
msgid "E_D_OVERHEAD_PRESS_CABLE_TIP_ONE"
msgstr "Do not lock out your elbows at the top."
msgid "E_D_OVERHEAD_PRESS_CABLE_TIP_TWO"
msgstr "Keep your knees slightly bent throughout the movement."
msgid "E_D_OVERHEAD_PRESS_CABLE_TIP_THREE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_OVERHEAD_PRESS_DUMBBELL_TITLE"
msgstr "Overhead Press: Dumbbell"
msgid "E_D_OVERHEAD_PRESS_DUMBBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_OVERHEAD_PRESS_DUMBBELL_PREP_TWO"
msgstr "Hold a dumbbell in each hand at your sides with your elbows bent at 90 degrees and palms facing forward."
msgid "E_D_OVERHEAD_PRESS_DUMBBELL_EXEC_ONE"
msgstr "Exhale and squeeze your shoulders to lift dumbbells vertically until your arms are straight overhead."
msgid "E_D_OVERHEAD_PRESS_DUMBBELL_EXEC_TWO"
msgstr "Pause for a few seconds, inhale, return to the starting position slowly and repeat."
msgid "E_D_OVERHEAD_PRESS_DUMBBELL_TIP_ONE"
msgstr "Do not lock out your elbows at the top."
msgid "E_D_OVERHEAD_PRESS_DUMBBELL_TIP_TWO"
msgstr "Keep your knees slightly bent throughout the movement."
msgid "E_D_OVERHEAD_PRESS_DUMBBELL_TIP_THREE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_OVERHEAD_PRESS_KETTLEBELL_TITLE"
msgstr "Overhead Press: Kettlebell"
msgid "E_D_OVERHEAD_PRESS_KETTLEBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_OVERHEAD_PRESS_KETTLEBELL_PREP_TWO"
msgstr "Hold a kettlebell in each hand with elbows bent at around 45 degrees at your sides and palms facing forward."
msgid "E_D_OVERHEAD_PRESS_KETTLEBELL_EXEC_ONE"
msgstr "Exhale and squeeze your shoulders to press the kettlebells vertically until your arms are straight overhead."
msgid "E_D_OVERHEAD_PRESS_KETTLEBELL_EXEC_TWO"
msgstr "Pause for a few seconds, inhale, return to the starting position slowly and repeat."
msgid "E_D_OVERHEAD_PRESS_KETTLEBELL_TIP_ONE"
msgstr "Do not lock out your elbows at the top."
msgid "E_D_OVERHEAD_PRESS_KETTLEBELL_TIP_TWO"
msgstr "Keep your knees slightly bent throughout the movement."
msgid "E_D_OVERHEAD_PRESS_KETTLEBELL_TIP_THREE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_OVERHEAD_PRESS_SMITH_TITLE"
msgstr "Overhead Press: Smith Machine"
msgid "E_D_OVERHEAD_PRESS_SMITH_PREP_ONE"
msgstr "Stand in the machine with feet shoulder width apart."
msgid "E_D_OVERHEAD_PRESS_SMITH_PREP_TWO"
msgstr "Hold the barbell in front of your shoulders with hands slightly wider than shoulder width apart and palms facing forward."
msgid "E_D_OVERHEAD_PRESS_SMITH_EXEC_ONE"
msgstr "Exhale and squeeze your shoulders to lift the bar vertically until your arms are straight overhead."
msgid "E_D_OVERHEAD_PRESS_SMITH_EXEC_TWO"
msgstr "Pause for a few seconds, inhale, return to the starting position slowly and repeat."
msgid "E_D_OVERHEAD_PRESS_SMITH_TIP_ONE"
msgstr "Do not lock out your elbows at the top."
msgid "E_D_OVERHEAD_PRESS_SMITH_TIP_TWO"
msgstr "Do not rest the bar on your shoulders."
msgid "E_D_OVERHEAD_PRESS_SMITH_TIP_THREE"
msgstr "Keep your knees slightly bent throughout the movement."
msgid "E_D_OVERHEAD_PRESS_SMITH_TIP_FOUR"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_OVERHEAD_SQUAT_BARBELL_TITLE"
msgstr "Overhead Press: Barbell (Squat Variation)"
msgid "E_D_OVERHEAD_SQUAT_BARBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_OVERHEAD_SQUAT_BARBELL_PREP_TWO"
msgstr "Hold the barbell overhead with hands slightly wider than shoulder width apart and arms fully extended."
msgid "E_D_OVERHEAD_SQUAT_BARBELL_EXEC_ONE"
msgstr "Squat down slowly with the bar resting overhead until your thighs are horizontal."
msgid "E_D_OVERHEAD_SQUAT_BARBELL_EXEC_TWO"
msgstr "Pause for a few seconds and return to the starting position slowly and repeat."
msgid "E_D_OVERHEAD_SQUAT_BARBELL_TIP_ONE"
msgstr "Do not let your knees go over your toes."
msgid "E_D_OVERHEAD_SQUAT_BARBELL_TIP_TWO"
msgstr "Keep the bar moving vertically."
msgid "E_D_OVERHEAD_SQUAT_BARBELL_TIP_THREE"
msgstr "To add difficulty, add weights gradually."
msgid "E_D_OVERHEAD_SQUAT_BARBELL_TIP_FOUR"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_OVERHEAD_TRICEPS_BAND_TITLE"
msgstr "Overhead Triceps Extension: Band"
msgid "E_D_OVERHEAD_TRICEPS_BAND_PREP_ONE"
msgstr "Take a step forward and bend your knees slightly."
msgid "E_D_OVERHEAD_TRICEPS_BAND_PREP_TWO"
msgstr "Hold the handles behind the back of your head in an overhand grip with arms bent and elbows lifted."
msgid "E_D_OVERHEAD_TRICEPS_BAND_EXEC_ONE"
msgstr "Exhale and extend your elbows to press your hands up until your arms are straight."
msgid "E_D_OVERHEAD_TRICEPS_BAND_EXEC_TWO"
msgstr "Pause for a few seconds, inhale, return to the starting position slowly and repeat."
msgid "E_D_OVERHEAD_TRICEPS_BAND_TIP_ONE"
msgstr "Keep your upper arms still and in line with your body."
msgid "E_D_OVERHEAD_TRICEPS_BAND_TIP_TWO"
msgstr "Do not lock out your elbows at the top."
msgid "E_D_OVERHEAD_TRICEPS_BAND_TIP_THREE"
msgstr "Do not shrug your shoulders throughout the movement."
msgid "E_D_OVERHEAD_TRICEPS_BAND_TIP_FOUR"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_OVERHEAD_TRICEPS_BARBELL_TITLE"
msgstr "Overhead Triceps Extension: Barbell"
msgid "E_D_OVERHEAD_TRICEPS_BARBELL_PREP_ONE"
msgstr "Sit on a bench with feet on the floor."
msgid "E_D_OVERHEAD_TRICEPS_BARBELL_PREP_TWO"
msgstr "Hold the barbell behind the back of your head in an overhand grip with arms bent and elbows lifted."
msgid "E_D_OVERHEAD_TRICEPS_BARBELL_EXEC_ONE"
msgstr "Exhale and extend your elbows to press your hands up until your arms are straight."
msgid "E_D_OVERHEAD_TRICEPS_BARBELL_EXEC_TWO"
msgstr "Pause for a few seconds, inhale, return to the starting position slowly and repeat."
msgid "E_D_OVERHEAD_TRICEPS_BARBELL_TIP_ONE"
msgstr "Keep your upper arms still and in line with your body."
msgid "E_D_OVERHEAD_TRICEPS_BARBELL_TIP_TWO"
msgstr "Do not lock out your elbows at the top."
msgid "E_D_OVERHEAD_TRICEPS_BARBELL_TIP_THREE"
msgstr "Do not shrug your shoulders throughout the movement."
msgid "E_D_OVERHEAD_TRICEPS_BARBELL_TIP_FOUR"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_OVERHEAD_TRICEPS_CABLE_TITLE"
msgstr "Overhead Triceps Extension: Cable"
msgid "E_D_OVERHEAD_TRICEPS_CABLE_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_OVERHEAD_TRICEPS_CABLE_PREP_TWO"
msgstr "Hold the handles behind the back of your head with arms bent and elbows lifted."
msgid "E_D_OVERHEAD_TRICEPS_CABLE_EXEC_ONE"
msgstr "Exhale and extend your elbows to press your hands up until your arms are straight."
msgid "E_D_OVERHEAD_TRICEPS_CABLE_EXEC_TWO"
msgstr "Pause for a few seconds, inhale, return to the starting position slowly and repeat."
msgid "E_D_OVERHEAD_TRICEPS_CABLE_TIP_ONE"
msgstr "Keep your upper arms still and in line with your body."
msgid "E_D_OVERHEAD_TRICEPS_CABLE_TIP_TWO"
msgstr "Do not lock out your elbows at the top."
msgid "E_D_OVERHEAD_TRICEPS_CABLE_TIP_THREE"
msgstr "Do not shrug your shoulders throughout the movement."
msgid "E_D_OVERHEAD_TRICEPS_CABLE_TIP_FOUR"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_OVERHEAD_TRICEPS_DUMBBELL_TITLE"
msgstr "Overhead Triceps Extension Dumbbell"
msgid "E_D_OVERHEAD_TRICEPS_DUMBBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_OVERHEAD_TRICEPS_DUMBBELL_PREP_TWO"
msgstr "Hold a dumbbell using both hands behind the back of your head with arms bent and elbows lifted."
msgid "E_D_OVERHEAD_TRICEPS_DUMBBELL_EXEC_ONE"
msgstr "Exhale and extend your elbows to press your hands up until your arms are straight."
msgid "E_D_OVERHEAD_TRICEPS_DUMBBELL_EXEC_TWO"
msgstr "Pause for a few seconds, inhale, return to the starting position slowly and repeat."
msgid "E_D_OVERHEAD_TRICEPS_DUMBBELL_TIP_ONE"
msgstr "Keep your upper arms still and in line with your body."
msgid "E_D_OVERHEAD_TRICEPS_DUMBBELL_TIP_TWO"
msgstr "Do not lock out your elbows at the top."
msgid "E_D_OVERHEAD_TRICEPS_DUMBBELL_TIP_THREE"
msgstr "Do not shrug your shoulders throughout the movement."
msgid "E_D_OVERHEAD_TRICEPS_DUMBBELL_TIP_FOUR"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_PEC_DECK_FLY_MACHINE_TITLE"
msgstr "Pec Deck Fly: Machine"
msgid "E_D_PEC_DECK_FLY_MACHINE_PREP_ONE"
msgstr "Sit on the machine with back supported and feet on the floor."
msgid "E_D_PEC_DECK_FLY_MACHINE_PREP_TWO"
msgstr "Hold the handles with palms facing forward."
msgid "E_D_PEC_DECK_FLY_MACHINE_PREP_THREE"
msgstr "Bend your elbows at 90 degrees with forearms against the pads."
msgid "E_D_PEC_DECK_FLY_MACHINE_EXEC_ONE"
msgstr "Exhale, squeeze your chest and press the handles together as close as possible."
msgid "E_D_PEC_DECK_FLY_MACHINE_EXEC_TWO"
msgstr "Pause for a few seconds, inhale and return to the starting position slowly and repeat."
msgid "E_D_PEC_DECK_FLY_MACHINE_TIP_ONE"
msgstr "Do not let your hands touch each other during the exercise."
msgid "E_D_PEC_DECK_FLY_MACHINE_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_PENDLAY_ROW_BARBELL_TITLE"
msgstr "Pendlay Row Barbell"
msgid "E_D_PENDLAY_ROW_BARBELL_PREP_ONE"
msgstr "Bend your knees slightly and lower your torso until horizontal."
msgid "E_D_PENDLAY_ROW_BARBELL_PREP_TWO"
msgstr "Hold the barbell with hands slightly wider than shoulder width apart in an overhand grip."
msgid "E_D_PENDLAY_ROW_BARBELL_EXEC_ONE"
msgstr "Squeeze your lats and shoulder blades to pull the bar vertically to your rib cage."
msgid "E_D_PENDLAY_ROW_BARBELL_EXEC_TWO"
msgstr "Return to the starting position slowly and repeat."
msgid "E_D_PENDLAY_ROW_BARBELL_TIP_ONE"
msgstr "To add difficulty, add weights gradually."
msgid "E_D_PENDLAY_ROW_BARBELL_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_PIKE_PUSH_UP_TITLE"
msgstr "Pike Push-up"
msgid "E_D_PIKE_PUSH_UP_PREP_ONE"
msgstr "Rest your hands shoulder width apart and feet on the floor."
msgid "E_D_PIKE_PUSH_UP_PREP_TWO"
msgstr "Bend your hips and form a pike shape with your body."
msgid "E_D_PIKE_PUSH_UP_PREP_THREE"
msgstr "Stretch your arms and lift your heels off the ground."
msgid "E_D_PIKE_PUSH_UP_EXEC_ONE"
msgstr "Inhale and bend the elbows to lower your body down until your head nearly touches the ground."
msgid "E_D_PIKE_PUSH_UP_EXEC_TWO"
msgstr "Exhale and return to the starting position and repeat."
msgid "E_D_PIKE_PUSH_UP_TIP_ONE"
msgstr "Do not fully extend your arms."
msgid "E_D_PIKE_PUSH_UP_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_PIKE_PUSH_UP_FEET_ON_BENCH_TITLE"
msgstr "Pike Push-up Feet On Bench"
msgid "E_D_PIKE_PUSH_UP_FEET_ON_BENCH_PREP_ONE"
msgstr "Rest your feet on a bench and hands on the floor."
msgid "E_D_PIKE_PUSH_UP_FEET_ON_BENCH_PREP_TWO"
msgstr "Bend your hips and form a pike shape with your body."
msgid "E_D_PIKE_PUSH_UP_FEET_ON_BENCH_PREP_THREE"
msgstr "Stretch your arms and legs."
msgid "E_D_PIKE_PUSH_UP_FEET_ON_BENCH_EXEC_ONE"
msgstr "Inhale and bend the elbows to lower your body down until your head nearly touches the ground."
msgid "E_D_PIKE_PUSH_UP_FEET_ON_BENCH_EXEC_TWO"
msgstr "Exhale and return to the starting position and repeat."
msgid "E_D_PIKE_PUSH_UP_FEET_ON_BENCH_TIP_ONE"
msgstr "Do not fully extend your arms."
msgid "E_D_PIKE_PUSH_UP_FEET_ON_BENCH_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_PISTOL_SQUAT_TITLE"
msgstr "Pistol Squat"
msgid "E_D_PISTOL_SQUAT_PREP_ONE"
msgstr "Stand with feet shoulder width apart and one foot lifted off the ground."
msgid "E_D_PISTOL_SQUAT_EXEC_ONE"
msgstr "Bend the knee of the standing leg to lower your hips until your thighs are parallel to the floor."
msgid "E_D_PISTOL_SQUAT_EXEC_TWO"
msgstr "Push through the heel and midfoot into the floor to the starting position slowly and switch to the other side and repeat the same repetitions."
msgid "E_D_PISTOL_SQUAT_TIP_ONE"
msgstr "Do not let your extended leg touch the ground throughout the movement."
msgid "E_D_PISTOL_SQUAT_TIP_TWO"
msgstr "Do not lock out your knees at the top."
msgid "E_D_PISTOL_SQUAT_TIP_THREE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_PISTOL_SQUAT_TIP_FOUR"
msgstr "Keep your glutes, quadriceps and hamstrings engaged."
msgid "E_D_PLANK_TITLE"
msgstr "Plank"
msgid "E_D_PLANK_PREP_ONE"
msgstr "Lie prone with your elbows directly under your shoulders, legs extended and toes on the floor."
msgid "E_D_PLANK_EXEC_ONE"
msgstr "Push your body up with toes and forearms on the floor."
msgid "E_D_PLANK_EXEC_TWO"
msgstr "Keep this position as long as you can."
msgid "E_D_PLANK_TIP_ONE"
msgstr "Keep your head, torso and thighs in a straight line."
msgid "E_D_PLANK_TIP_TWO"
msgstr "Keep a braced core during the exercise."
msgid "E_D_PLANK_TIP_THREE"
msgstr "Keep your shoulders down and do not put the body weight on your shoulders."
msgid "E_D_POWER_CLEAN_BARBELL_TITLE"
msgstr "Power Clean: Barbell"
msgid "E_D_POWER_CLEAN_BARBELL_PREP_ONE"
msgstr "Squat down with feet shoulder width apart and hold the barbell with hands shoulder width apart in an overhand grip."
msgid "E_D_POWER_CLEAN_BARBELL_EXEC_ONE"
msgstr "Drive your hips up and straighten your legs to pull the barbell up."
msgid "E_D_POWER_CLEAN_BARBELL_EXEC_TWO"
msgstr "Once the bar passes your mid-thigh, jump slightly and drop your body under the bar with your elbows facing forward and palms facing up."
msgid "E_D_POWER_CLEAN_BARBELL_EXEC_THREE"
msgstr "As you receive the bar, slightly bend your knees."
msgid "E_D_POWER_CLEAN_BARBELL_EXEC_FOUR"
msgstr "Straighten your knees and hips to stand up straight."
msgid "E_D_POWER_CLEAN_BARBELL_EXEC_FIVE"
msgstr "Lower the barbell to the starting position slowly and repeat."
msgid "E_D_POWER_CLEAN_BARBELL_TIP_ONE"
msgstr "To add difficulty, add weights gradually."
msgid "E_D_POWER_CLEAN_BARBELL_TIP_TWO"
msgstr "Keep your knees and elbows slightly bent during the exercise."
msgid "E_D_POWER_CLEAN_BARBELL_TIP_THREE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_POWER_PUSH_AWAY_TITLE"
msgstr "Power Push Away"
msgid "E_D_POWER_PUSH_AWAY_PREP_ONE"
msgstr "Kneel on the floor with feet and knees on the floor."
msgid "E_D_POWER_PUSH_AWAY_PREP_TWO"
msgstr "Extend your arms in front of you and keep them close to your ears."
msgid "E_D_POWER_PUSH_AWAY_EXEC_ONE"
msgstr "Inhale and extend the knees to push your body forward."
msgid "E_D_POWER_PUSH_AWAY_EXEC_TWO"
msgstr "Bend the elbows to support your body."
msgid "E_D_POWER_PUSH_AWAY_EXEC_THREE"
msgstr "Exhale and return to the starting position and repeat."
msgid "E_D_POWER_PUSH_AWAY_TIP_ONE"
msgstr "Do not fully extend your arms."
msgid "E_D_POWER_PUSH_AWAY_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_POWER_PUSH_AWAY_TIP_THREE"
msgstr "Do not let your chest touch the floor."
msgid "E_D_POWER_SNATCH_BARBELL_TITLE"
msgstr "Power Snatch: Barbell"
msgid "E_D_POWER_SNATCH_BARBELL_FOCUS"
msgstr "Full Body"
msgid "E_D_POWER_SNATCH_BARBELL_EQUIP"
msgstr "Barbell"
msgid "E_D_POWER_SNATCH_BARBELL_PREP_ONE"
msgstr "Squat down with feet shoulder width apart and hold the barbell with hands wider than shoulder width apart in an overhand grip."
msgid "E_D_POWER_SNATCH_BARBELL_EXEC_ONE"
msgstr "Shrug your shoulders to lift the barbell with your arms straight."
msgid "E_D_POWER_SNATCH_BARBELL_EXEC_TWO"
msgstr "Once the bar passes your mid-thigh, jump slightly and drop your body under the bar while lifting the barbell overhead with palms facing forward."
msgid "E_D_POWER_SNATCH_BARBELL_EXEC_THREE"
msgstr "As you lift the barbell, slightly bend your knees."
msgid "E_D_POWER_SNATCH_BARBELL_EXEC_FOUR"
msgstr "Brace you core and drive through your feet to stand up straight."
msgid "E_D_POWER_SNATCH_BARBELL_EXEC_FIVE"
msgstr "Lower the barbell to the starting position slowly and repeat."
msgid "E_D_POWER_SNATCH_BARBELL_TIP_ONE"
msgstr "To add difficulty, add weights gradually."
msgid "E_D_POWER_SNATCH_BARBELL_TIP_TWO"
msgstr "Keep your knees and elbows slightly bent during the exercise."
msgid "E_D_POWER_SNATCH_BARBELL_TIP_THREE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_PRAYER_PUSH_TITLE"
msgstr "Prayer Push"
msgid "E_D_PRAYER_PUSH_FOCUS"
msgstr "Chest, Shoulders"
msgid "E_D_PRAYER_PUSH_EQUIP"
msgstr "Bodyweight"
msgid "E_D_PRAYER_PUSH_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_PRAYER_PUSH_PREP_TWO"
msgstr "Clasp your hands in front of your chest."
msgid "E_D_PRAYER_PUSH_EXEC_ONE"
msgstr "Press your hands up and raise your arms overhead."
msgid "E_D_PRAYER_PUSH_EXEC_TWO"
msgstr "Lower your hands back to the starting position and repeat."
msgid "E_D_PRAYER_PUSH_TIP_ONE"
msgstr "Control and stabilize your arms during the exercise"
msgid "E_D_PRAYER_PUSH_TIP_TWO"
msgstr "Keep a braced core during the exercise"
msgid "E_D_PREACHER_CURL_BARBELL_TITLE"
msgstr "Preacher Curl: Barbell"
msgid "E_D_PREACHER_CURL_BARBELL_FOCUS"
msgstr "Biceps, Forearms"
msgid "E_D_PREACHER_CURL_BARBELL_EQUIP"
msgstr "Barbell, Preacher Bench"
msgid "E_D_PREACHER_CURL_BARBELL_PREP_ONE"
msgstr "Sit on a bench with feet on the floor."
msgid "E_D_PREACHER_CURL_BARBELL_PREP_TWO"
msgstr "Hold the barbell in a shoulder width underhand grip with arms extended against the pad."
msgid "E_D_PREACHER_CURL_BARBELL_EXEC_ONE"
msgstr "Exhale, squeeze your biceps and bend you elbows to bring the bar close to your shoulders."
msgid "E_D_PREACHER_CURL_BARBELL_EXEC_TWO"
msgstr "Pause for a few seconds, inhale and return to the starting position slowly and repeat."
msgid "E_D_PREACHER_CURL_BARBELL_TIP_ONE"
