msgid "E_D_LAT_PULLDOWN_CLOSE_GRIP_CABLE_FOCUS"
msgstr "Back, Traps, Biceps"
msgid "E_D_LAT_PULLDOWN_CLOSE_GRIP_CABLE_EQUIP"
msgstr "Cable Machine, Flat Bench"
