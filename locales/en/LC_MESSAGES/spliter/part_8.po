msgid "E_D_INVERTED_ROW_TITLE"
msgstr "Inverted Row"
msgid "E_D_INVERTED_ROW_FOCUS"
msgstr "Back, Traps, Biceps"
msgid "E_D_INVERTED_ROW_EQUIP"
msgstr "Bodyweight"
msgid "E_D_INVERTED_ROW_PREP_ONE"
msgstr "Hold the bar with your hands shoulder width apart in an overhand grip and arms extended."
msgid "E_D_INVERTED_ROW_PREP_TWO"
msgstr "Lie under the rack with body off the ground in a line and supported by the heels."
msgid "E_D_INVERTED_ROW_EXEC_ONE"
msgstr "Squeeze your lats and bend your elbows to pull yourself up to the bar until your chest touches the bar."
msgid "E_D_INVERTED_ROW_EXEC_TWO"
msgstr "Return to the starting position slowly and repeat."
msgid "E_D_INVERTED_ROW_TIP_ONE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_INVERTED_ROW_REVERSE_GRIP_TITLE"
msgstr "Inverted Row Reverse Grip"
msgid "E_D_INVERTED_ROW_REVERSE_GRIP_FOCUS"
msgstr "Back, Traps, Biceps"
msgid "E_D_INVERTED_ROW_REVERSE_GRIP_EQUIP"
msgstr "Bodyweight"
msgid "E_D_INVERTED_ROW_REVERSE_GRIP_PREP_ONE"
msgstr "Hold the bar with your hands shoulder width apart in an underhand grip and arms extended."
msgid "E_D_INVERTED_ROW_REVERSE_GRIP_PREP_TWO"
msgstr "Lie under the rack with body off the ground in a line and supported by the heels."
msgid "E_D_INVERTED_ROW_REVERSE_GRIP_EXEC_ONE"
msgstr "Squeeze your lats and bend your elbows to pull yourself up to the bar until your chest touches the bar."
msgid "E_D_INVERTED_ROW_REVERSE_GRIP_EXEC_TWO"
msgstr "Return to the tarting position slowly and repeat."
msgid "E_D_INVERTED_ROW_REVERSE_GRIP_TIP_ONE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_JUMP_SQUAT_TITLE"
msgstr "Jump Squat"
msgid "E_D_JUMP_SQUAT_FOCUS"
msgstr "Glutes, Quadriceps, Calves"
msgid "E_D_JUMP_SQUAT_EQUIP"
msgstr "Bodyweight"
msgid "E_D_JUMP_SQUAT_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_JUMP_SQUAT_PREP_TWO"
msgstr "Cross your arms over your chest."
msgid "E_D_JUMP_SQUAT_PREP_THREE"
msgstr "Squat down until your thighs are parallel to the floor."
msgid "E_D_JUMP_SQUAT_EXEC_ONE"
msgstr "Drive through heels, knees and hips to jump upward as high as possible."
msgid "E_D_JUMP_SQUAT_EXEC_TWO"
msgstr "Land on the floor and bend your knees to the starting position and repeat."
msgid "E_D_JUMP_SQUAT_TIP_ONE"
msgstr "To avoid joint injuries, keep your tiptoes bein the same direction with your knees."
msgid "E_D_JUMP_SQUAT_TIP_TWO"
msgstr "Do not let our knees go over your toes."
msgid "E_D_JUMP_SQUAT_TIP_THREE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_JUMP_SQUAT_BARBELL_TITLE"
msgstr "Jump Squat: Barbell"
msgid "E_D_JUMP_SQUAT_BARBELL_FOCUS"
msgstr "Quadriceps, Glutes, Calves"
msgid "E_D_JUMP_SQUAT_BARBELL_EQUIP"
msgstr "Barbell"
msgid "E_D_JUMP_SQUAT_BARBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_JUMP_SQUAT_BARBELL_PREP_TWO"
msgstr "Place a barbell behind across your shoulders."
msgid "E_D_JUMP_SQUAT_BARBELL_PREP_THREE"
msgstr "Hold the bar with your palms wide apart and facing forward."
msgid "E_D_JUMP_SQUAT_BARBELL_EXEC_ONE"
msgstr "Squat down by bending at the hips and knees until the thighs are parallel to the floor."
msgid "E_D_JUMP_SQUAT_BARBELL_EXEC_TWO"
msgstr "Drive through heels, knees and hips to jump upward as high as possible."
msgid "E_D_JUMP_SQUAT_BARBELL_EXEC_THREE"
msgstr "Land on the floor and bend your knees to the starting position and repeat."
msgid "E_D_JUMP_SQUAT_BARBELL_TIP_ONE"
msgstr "To avoid joint injuries, keep your tiptoes bein the same direction with your knees."
msgid "E_D_JUMP_SQUAT_BARBELL_TIP_TWO"
msgstr "Do not let your knees go over your toes."
msgid "E_D_JUMP_SQUAT_BARBELL_TIP_THREE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_JUMP_SQUAT_DUMBBELL_TITLE"
msgstr "Jump Squat- Dumbbell"
msgid "E_D_JUMP_SQUAT_DUMBBELL_FOCUS"
msgstr "Quadriceps, Glutes, Calves"
msgid "E_D_JUMP_SQUAT_DUMBBELL_EQUIP"
msgstr "Dumbbell"
msgid "E_D_JUMP_SQUAT_DUMBBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_JUMP_SQUAT_DUMBBELL_PREP_TWO"
msgstr "Hold a dumbbell in each hand by your sides with arms extended."
msgid "E_D_JUMP_SQUAT_DUMBBELL_PREP_THREE"
msgstr "Squat down until your thighs are parallel to the floor."
msgid "E_D_JUMP_SQUAT_DUMBBELL_EXEC_ONE"
msgstr "Drive through heels, knees and hips to jump upward as high as possible."
msgid "E_D_JUMP_SQUAT_DUMBBELL_EXEC_TWO"
msgstr "Land on the floor and bend your knees to the starting position and repeat."
msgid "E_D_JUMP_SQUAT_DUMBBELL_TIP_ONE"
msgstr "To avoid joint injuries, keep your tiptoes bein the same direction with your knees."
msgid "E_D_JUMP_SQUAT_DUMBBELL_TIP_TWO"
msgstr "Do not let your knees go over your toes."
msgid "E_D_JUMP_SQUAT_DUMBBELL_TIP_THREE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_JUMPING_JACK_TITLE"
msgstr "Jumping Jack"
msgid "E_D_JUMPING_JACK_FOCUS"
msgstr "Full Body"
msgid "E_D_JUMPING_JACK_EQUIP"
msgstr "Bodyweight"
msgid "E_D_JUMPING_JACK_PREP_ONE"
msgstr "Stand with your legs close together and arms straight down at your sides."
msgid "E_D_JUMPING_JACK_EXEC_ONE"
msgstr "Jump as you move your feet out to sides shoulder width apart and swing your arms overhead in an arc motion."
msgid "E_D_JUMPING_JACK_EXEC_TWO"
msgstr "Jump back to the starting position and repeat."
msgid "E_D_JUMPING_JACK_TIP_ONE"
msgstr "Do not tilt your arms and legs forward or backward."
msgid "E_D_JUMPING_JACK_TIP_TWO"
msgstr "Keep your knees slightly bent throughout the movement."
msgid "E_D_JUMPING_JACK_TIP_THREE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_KETTLEBELL_SWING_TITLE"
msgstr "Kettlebell Swing"
msgid "E_D_KETTLEBELL_SWING_FOCUS"
msgstr "Full Body"
msgid "E_D_KETTLEBELL_SWING_EQUIP"
msgstr "Kettlebell"
msgid "E_D_KETTLEBELL_SWING_PREP_ONE"
msgstr "Bend at your waist and knees with feet shoulder width apart and torso nearly horizontal."
msgid "E_D_KETTLEBELL_SWING_PREP_TWO"
msgstr "Hold the kettlebell with both hands between your legs."
msgid "E_D_KETTLEBELL_SWING_EXEC_ONE"
msgstr "Contract your glutes and thrust your hips forward to swing the kettlebell up to your chest level."
msgid "E_D_KETTLEBELL_SWING_EXEC_TWO"
msgstr "Swing the kettlebell back down to the starting position and repeat."
msgid "E_D_KETTLEBELL_SWING_TIP_ONE"
msgstr "Rely on your glutes and quads to swing the kettlebell up instead of your arms or back."
msgid "E_D_KETTLEBELL_SWING_TIP_TWO"
msgstr "Keep the speed and height controlled and do not swing too fast or drop too low."
msgid "E_D_KETTLEBELL_SWING_TIP_THREE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_KNEE_PUSH_UP_TITLE"
msgstr "Knee Push-up"
msgid "E_D_KNEE_PUSH_UP_FOCUS"
msgstr "Chest, Triceps, Shoulders"
msgid "E_D_KNEE_PUSH_UP_EQUIP"
msgstr "Bodyweight"
msgid "E_D_KNEE_PUSH_UP_PREP_ONE"
msgstr "Lie prone on the floor with feet and knees on the floor."
msgid "E_D_KNEE_PUSH_UP_PREP_TWO"
msgstr "Extend your arms and hands slightly wider than shoulder width apart."
msgid "E_D_KNEE_PUSH_UP_EXEC_ONE"
msgstr "Inhale and lower your body down until your chest almost touches the floor."
msgid "E_D_KNEE_PUSH_UP_EXEC_TWO"
msgstr "Exhale and raise your body back to the starting position and repeat."
msgid "E_D_KNEE_PUSH_UP_TIP_ONE"
msgstr "Do not fully extend your arms,"
msgid "E_D_KNEE_PUSH_UP_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_KNEE_PUSH_UP_TIP_THREE"
msgstr "Do not let your chest touch the floor."
msgid "E_D_KNEE_RAISE_TITLE"
msgstr "Knee Raise"
msgid "E_D_KNEE_RAISE_FOCUS"
msgstr "Abs, Quadriceps"
msgid "E_D_KNEE_RAISE_EQUIP"
msgstr "Bodyweight"
msgid "E_D_KNEE_RAISE_PREP_ONE"
msgstr "Hold the handles and have your forearms and back against the pads of the machine."
msgid "E_D_KNEE_RAISE_PREP_TWO"
msgstr "Keep your elbows bent at 90 degrees and legs extended straight down."
msgid "E_D_KNEE_RAISE_EXEC_ONE"
msgstr "Exhale and squeeze your abs to lift your knees up to your chest in an are."
msgid "E_D_KNEE_RAISE_EXEC_TWO"
msgstr "Pause for a few seconds, inhale and return to the starting position slowly and repeat."
msgid "E_D_KNEE_RAISE_TIP_ONE"
msgstr "Keep your legs together and do not swing them up to create any momentum."
msgid "E_D_KNEE_RAISE_TIP_TWO"
msgstr "Keep a braced core during the exercise."
msgid "E_D_KNEE_RAISE_TWIST_TITLE"
msgstr "Knee Raise Twist"
msgid "E_D_KNEE_RAISE_TWIST_FOCUS"
msgstr "Abs, Quadriceps"
msgid "E_D_KNEE_RAISE_TWIST_EQUIP"
msgstr "Bodyweight"
msgid "E_D_KNEE_RAISE_TWIST_PREP_ONE"
msgstr "Hold the handles and have your forearms and back against the pads of the machine."
msgid "E_D_KNEE_RAISE_TWIST_PREP_TWO"
msgstr "Keep your elbows bent at 90 degrees and legs extended straight down."
msgid "E_D_KNEE_RAISE_TWIST_EXEC_ONE"
msgstr "Exhale and squeeze your obliques to lift your knees up and over to one side."
msgid "E_D_KNEE_RAISE_TWIST_EXEC_TWO"
msgstr "Pause for a few seconds, inhale, drop your legs."
msgid "E_D_KNEE_RAISE_TWIST_EXEC_THREE"
msgstr "back down to the starting position and complete alternating between left and right."
msgid "E_D_KNEE_RAISE_TWIST_TIP_ONE"
msgstr "Keep your legs together and do not swing them up to create any momentum."
msgid "E_D_KNEE_RAISE_TWIST_TIP_TWO"
msgstr "Keep a braced core during the exercise."
msgid "E_D_KNEELING_CRUNCH_BAND_TITLE"
msgstr "Kneeling Crunch: Band"
msgid "E_D_KNEELING_CRUNCH_BAND_FOCUS"
msgstr "Abs"
msgid "E_D_KNEELING_CRUNCH_BAND_EQUIP"
msgstr "Handle Band"
msgid "E_D_KNEELING_CRUNCH_BAND_PREP_ONE"
msgstr "Secure the band to a high position and kneel down back to it."
msgid "E_D_KNEELING_CRUNCH_BAND_PREP_TWO"
msgstr "Lean slightly forward and hold the handles in a narrow underhand grip in front of your shoulders."
msgid "E_D_KNEELING_CRUNCH_BAND_EXEC_ONE"
msgstr "Exhale, squeeze your abs and bend at your waist to bring your torso towards your thighs."
msgid "E_D_KNEELING_CRUNCH_BAND_EXEC_TWO"
msgstr "Pause at the bottom for a few seconds, inhale and return to the starting position slowly and repeat."
msgid "E_D_KNEELING_CRUNCH_BAND_TIP_ONE"
msgstr "Crunch your upper body down using your abs rather than pulling the handles down with your hands."
msgid "E_D_KNEELING_CRUNCH_BAND_TIP_TWO"
msgstr "Keep your core braced and hips still during the. exercise."
msgid "E_D_KNEELING_CRUNCH_CABLE_TITLE"
msgstr "Kneeling Crunch: Cable"
msgid "E_D_KNEELING_CRUNCH_CABLE_FOCUS"
msgstr "Abs"
msgid "E_D_KNEELING_CRUNCH_CABLE_EQUIP"
msgstr "Cable Machine"
msgid "E_D_KNEELING_CRUNCH_CABLE_PREP_ONE"
msgstr "Hold the rope attachment connected to a high pulley and kneel down."
msgid "E_D_KNEELING_CRUNCH_CABLE_PREP_TWO"
msgstr "Position your wrists against your forehead."
msgid "E_D_KNEELING_CRUNCH_CABLE_EXEC_ONE"
msgstr "Exhale, squeeze your abs and bend at your waist to lower your torso until your elbows meet the middle thighs,"
msgid "E_D_KNEELING_CRUNCH_CABLE_EXEC_TWO"
msgstr "Inhale and slowly raise yourself back to the preparation position and repeat."
msgid "E_D_KNEELING_CRUNCH_CABLE_TIP_ONE"
msgstr "Crunch the weight down using your abs rather than pulling the rope down with your hands."
msgid "E_D_KNEELING_CRUNCH_CABLE_TIP_TWO"
msgstr "To add difficulty, add weights gradually."
msgid "E_D_KNEELING_CRUNCH_CABLE_TIP_THREE"
msgstr "Keep your core braced and hips still during the exercise."
msgid "E_D_KNEELING_PLANK_TITLE"
msgstr "Kneeling Plank"
msgid "E_D_KNEELING_PLANK_FOCUS"
msgstr "Abs, Glutes"
msgid "E_D_KNEELING_PLANK_EQUIP"
msgstr "Bodyweight"
msgid "E_D_KNEELING_PLANK_PREP_ONE"
msgstr "Lie prone with your elbows bent at 90 degrees and directly under your shoulders, legs extended."
msgid "E_D_KNEELING_PLANK_EXEC_ONE"
msgstr "Push your body up with knees and forearms on the floor."
msgid "E_D_KNEELING_PLANK_EXEC_TWO"
msgstr "Lift the feet up high until your knees are bent at 90 degrees."
msgid "E_D_KNEELING_PLANK_EXEC_THREE"
msgstr "Keep this position as long as you can."
msgid "E_D_KNEELING_PLANK_TIP_ONE"
msgstr "Keep your head, torso and thighs in a straight line."
msgid "E_D_KNEELING_PLANK_TIP_TWO"
msgstr "Keep a braced core during the exercise."
msgid "E_D_KNEELING_PLANK_TIP_THREE"
msgstr "Keep your shoulders down and do not put the body weight on your shoulders."
msgid "E_D_KNEELING_PULL_DOWN_BAND_TITLE"
msgstr "Kneeling Pull-down: Band"
msgid "E_D_KNEELING_PULL_DOWN_BAND_FOCUS"
msgstr "Back, Traps, Shoulders"
msgid "E_D_KNEELING_PULL_DOWN_BAND_EQUIP"
msgstr "Handle Band"
msgid "E_D_KNEELING_PULL_DOWN_BAND_PREP_ONE"
msgstr "Secure the band to a high object."
msgid "E_D_KNEELING_PULL_DOWN_BAND_PREP_TWO"
msgstr "Kneel down on one knee."
msgid "E_D_KNEELING_PULL_DOWN_BAND_PREP_THREE"
msgstr "Hold handles with hands shoulder width apart in an overhand grip."
msgid "E_D_KNEELING_PULL_DOWN_BAND_PREP_FOUR"
msgstr "Extend your arms upward."
msgid "E_D_KNEELING_PULL_DOWN_BAND_EXEC_ONE"
msgstr "Contract your lats and bend your elbows to pull the handles down to your shoulders."
msgid "E_D_KNEELING_PULL_DOWN_BAND_EXEC_TWO"
msgstr "Pause for a few seconds and return to the starting position slowly and repeat."
msgid "E_D_KNEELING_PULL_DOWN_BAND_TIP_ONE"
msgstr "Do not rely on the mechanical impulse to assist the movement."
msgid "E_D_KNEELING_PULL_DOWN_BAND_TIP_TWO"
msgstr "Keep the upward movement controlled to avoid pulling yourself out of position."
msgid "E_D_KNEELING_TORSO_ROTATION_MACHINE_TITLE"
msgstr "Kneeling Torso Rotation: Machine"
msgid "E_D_KNEELING_TORSO_ROTATION_MACHINE_FOCUS"
msgstr "Abs"
msgid "E_D_KNEELING_TORSO_ROTATION_MACHINE_EQUIP"
msgstr "Torso Rotation Machine"
msgid "E_D_KNEELING_TORSO_ROTATION_MACHINE_PREP_ONE"
msgstr "Kneel down on the machine with your chest and thighs against the pads."
msgid "E_D_KNEELING_TORSO_ROTATION_MACHINE_PREP_TWO"
msgstr "Hold the handles in a neutral grip."
