msgid "E_D_REVERSE_WRIST_CURL_BARBELL_TITLE"
msgstr "Reverse Wrist Curl: Barbell"
msgid "E_D_REVERSE_WRIST_CURL_BARBELL_FOCUS"
msgstr "Forearms"
msgid "E_D_REVERSE_WRIST_CURL_BARBELL_EQUIP"
msgstr "Barbell, Flat Bench"
msgid "E_D_REVERSE_WRIST_CURL_BARBELL_PREP_ONE"
msgstr "Sit on a bench and rest your forearms on the thighs with the wrists ahead of the knees."
msgid "E_D_REVERSE_WRIST_CURL_BARBELL_PREP_TWO"
msgstr "Hold a barbell with an overhand grip and curl the wrists downwards."
msgid "E_D_REVERSE_WRIST_CURL_BARBELL_EXEC_ONE"
msgstr "Extend the curled wrists and raise the barbell until you feel a stretch in your arms."
msgid "E_D_REVERSE_WRIST_CURL_BARBELL_EXEC_TWO"
msgstr "Return to the starting position and repeat."
msgid "E_D_REVERSE_WRIST_CURL_BARBELL_TIP_ONE"
msgstr "Keep your thighs and forearms parallel to the floor."
msgid "E_D_REVERSE_WRIST_CURL_BARBELL_TIP_TWO"
msgstr "Keep your elbows and wrists at the same height during the exercise."
msgid "E_D_REVERSE_WRIST_CURL_CABLE_TITLE"
msgstr "Reverse Wrist Curl: Cable"
msgid "E_D_REVERSE_WRIST_CURL_CABLE_FOCUS"
msgstr "Forearms"
msgid "E_D_REVERSE_WRIST_CURL_CABLE_EQUIP"
msgstr "Cable Machine, Flat Bench"
msgid "E_D_REVERSE_WRIST_CURL_CABLE_PREP_ONE"
msgstr "Sit on a bench before the machine with your forearms rested on the thighs."
msgid "E_D_REVERSE_WRIST_CURL_CABLE_PREP_TWO"
msgstr "Hold the handle with an overhand grip and curl the wrists downwards."
msgid "E_D_REVERSE_WRIST_CURL_CABLE_EXEC_ONE"
msgstr "Extend the curled wrists and raise the bar until you feel a stretch in your arms."
msgid "E_D_REVERSE_WRIST_CURL_CABLE_EXEC_TWO"
msgstr "Return to the starting position and repeat."
msgid "E_D_REVERSE_WRIST_CURL_CABLE_TIP_ONE"
msgstr "Keep your thighs and forearms parallel to the floor."
msgid "E_D_REVERSE_WRIST_CURL_CABLE_TIP_TWO"
msgstr "Keep your elbows and wrists at the same height during the exercise."
msgid "E_D_REVERSE_WRIST_CURL_DUMBBELL_TITLE"
msgstr "Reverse Wrist Curl: Dumbbell"
msgid "E_D_REVERSE_WRIST_CURL_DUMBBELL_FOCUS"
msgstr "Forearms"
msgid "E_D_REVERSE_WRIST_CURL_DUMBBELL_EQUIP"
msgstr "Dumbbell, Flat Bench"
msgid "E_D_REVERSE_WRIST_CURL_DUMBBELL_PREP_ONE"
msgstr "Sit on a bench and rest your forearms on the thighs."
msgid "E_D_REVERSE_WRIST_CURL_DUMBBELL_PREP_TWO"
msgstr "Hold a dumbbell in each hand with an overhand grip and curl the wrists downwards."
msgid "E_D_REVERSE_WRIST_CURL_DUMBBELL_EXEC_ONE"
msgstr "Extend the curled wrists and raise the dumbbells until you feel a stretch in your arms."
msgid "E_D_REVERSE_WRIST_CURL_DUMBBELL_EXEC_TWO"
msgstr "Return to the starting position and repeat."
msgid "E_D_REVERSE_WRIST_CURL_DUMBBELL_TIP_ONE"
msgstr "Keep your thighs and forearms parallel to the floor."
msgid "E_D_REVERSE_WRIST_CURL_DUMBBELL_TIP_TWO"
msgstr "Keep your elbows and wrists at the same height during the exercise."
msgid "E_D_ROMANIAN_DEADLIFT_BARBELL_TITLE"
msgstr "Romanian Deadlift: Barbell"
msgid "E_D_ROMANIAN_DEADLIFT_BARBELL_FOCUS"
msgstr "Lower back, Glutes, Hamstrings"
msgid "E_D_ROMANIAN_DEADLIFT_BARBELL_EQUIP"
msgstr "Barbell"
msgid "E_D_ROMANIAN_DEADLIFT_BARBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_ROMANIAN_DEADLIFT_BARBELL_PREP_TWO"
msgstr "Bend at hips and hold the barbell off the floor outside of your thighs with an overhand grip. Slightly bend your knees."
msgid "E_D_ROMANIAN_DEADLIFT_BARBELL_EXEC_ONE"
msgstr "Exhale and squeeze your glutes and tighten your lats to lift the barbell until standing up straight,"
msgid "E_D_ROMANIAN_DEADLIFT_BARBELL_EXEC_TWO"
msgstr "Rotate your shoulders back until feeling slightly stretched."
msgid "E_D_ROMANIAN_DEADLIFT_BARBELL_EXEC_THREE"
msgstr "Pause for a few seconds, inhale and return to the starting position slowly and repeat."
msgid "E_D_ROMANIAN_DEADLIFT_BARBELL_TIP_ONE"
msgstr "Position your hips between the level of shoulders and knees at the starting position."
msgid "E_D_ROMANIAN_DEADLIFT_BARBELL_TIP_TWO"
msgstr "Move the bar vertically."
msgid "E_D_ROMANIAN_DEADLIFT_BARBELL_TIP_THREE"
msgstr "Brace the abs to support your straight back throughout the movement."
msgid "E_D_ROMANIAN_DEADLIFT_BARBELL_TIP_FOUR"
msgstr "Do not pull upward using your arms or shoulders."
msgid "E_D_ROMANIAN_DEADLIFT_BARBELL_TIP_FIVE"
msgstr "Do not thrust your hips forward at the top position."
msgid "E_D_ROMANIAN_DEADLIFT_BARBELL_TIP_SIX"
msgstr "Do not let the bar touch the floor during the exercise."
msgid "E_D_ROMANIAN_DEADLIFT_DUMBBELL_TITLE"
msgstr "Romanian Deadlift Dumbbell"
msgid "E_D_ROMANIAN_DEADLIFT_DUMBBELL_FOCUS"
msgstr "Lower back, Glutes, Hamstrings"
msgid "E_D_ROMANIAN_DEADLIFT_DUMBBELL_EQUIP"
msgstr "Dumbbell"
msgid "E_D_ROMANIAN_DEADLIFT_DUMBBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_ROMANIAN_DEADLIFT_DUMBBELL_PREP_TWO"
msgstr "Bend at hips and hold dumbbells off the floor outside of your thighs with an overhand grip. Slightly bend your knees."
msgid "E_D_ROMANIAN_DEADLIFT_DUMBBELL_EXEC_ONE"
msgstr "Exhale and squeeze your glutes and tighten your lats to lift the dumbbells until standing up straight."
msgid "E_D_ROMANIAN_DEADLIFT_DUMBBELL_EXEC_TWO"
msgstr "Rotate your shoulders back until feeling slightly stretched."
msgid "E_D_ROMANIAN_DEADLIFT_DUMBBELL_EXEC_THREE"
msgstr "Pause for a few seconds, inhale and return to the starting position slowly and repeat."
msgid "E_D_ROMANIAN_DEADLIFT_DUMBBELL_TIP_ONE"
msgstr "Position your hips between the level of shoulders and knees at the starting position."
msgid "E_D_ROMANIAN_DEADLIFT_DUMBBELL_TIP_TWO"
msgstr "Move the dumbbells vertically."
msgid "E_D_ROMANIAN_DEADLIFT_DUMBBELL_TIP_THREE"
msgstr "Brace the abs to support your straight back throughout the movement."
msgid "E_D_ROMANIAN_DEADLIFT_DUMBBELL_TIP_FOUR"
msgstr "Do not pull upward using your arms or shoulders."
msgid "E_D_ROMANIAN_DEADLIFT_DUMBBELL_TIP_FIVE"
msgstr "Do not thrust your hips forward at the top position."
msgid "E_D_ROMANIAN_DEADLIFT_DUMBBELL_TIP_SIX"
msgstr "Do not let dumbbells touch the floor during the exercise."
msgid "E_D_ROW_IN_DOORWAY_TITLE"
msgstr "Row in Doorway"
msgid "E_D_ROW_IN_DOORWAY_FOCUS"
msgstr "Back, Traps, Biceps"
msgid "E_D_ROW_IN_DOORWAY_EQUIP"
msgstr "Bodyweight"
msgid "E_D_ROW_IN_DOORWAY_PREP_ONE"
msgstr "Stand with feet slightly apart and hold a door frame around upper abs height."
msgid "E_D_ROW_IN_DOORWAY_PREP_TWO"
msgstr "Lower your body back until arms are extended and keep your body in a straight line."
msgid "E_D_ROW_IN_DOORWAY_EXEC_ONE"
msgstr "Squeeze your lats and bend your elbows to pull yourself up towards the door."
msgid "E_D_ROW_IN_DOORWAY_EXEC_TWO"
msgstr "Pause at the top for a few seconds and return to the starting position slowly."
msgid "E_D_ROW_IN_DOORWAY_TIP_ONE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_RUSSIAN_TWIST_TITLE"
msgstr "Russian Twist"
msgid "E_D_RUSSIAN_TWIST_FOCUS"
msgstr "Abs"
msgid "E_D_RUSSIAN_TWIST_EQUIP"
msgstr "Bodyweight"
msgid "E_D_RUSSIAN_TWIST_PREP_ONE"
msgstr "Sit on the floor and lean slightly backward with knee bent and feet planted slightly apart."
msgid "E_D_RUSSIAN_TWIST_PREP_TWO"
msgstr "Clasp your hands over the chest."
msgid "E_D_RUSSIAN_TWIST_EXEC_ONE"
msgstr "Exhale and squeeze your obliques to twist your torso to one side."
msgid "E_D_RUSSIAN_TWIST_EXEC_TWO"
msgstr "Twist your torso back to the starting position."
msgid "E_D_RUSSIAN_TWIST_EXEC_THREE"
msgstr "Complete alternating between left and right."
msgid "E_D_RUSSIAN_TWIST_TIP_ONE"
msgstr "Do not use hands or arms to assist the movement."
msgid "E_D_RUSSIAN_TWIST_TIP_TWO"
msgstr "Keep your legs stationary throughout the movement."
msgid "E_D_RUSSIAN_TWIST_TIP_THREE"
msgstr "Keep a straight ack and braced core during the exercise."
msgid "E_D_RUSSIAN_TWIST_DUMBBELL_TITLE"
msgstr "Russian Twist: Dumbbell"
msgid "E_D_RUSSIAN_TWIST_DUMBBELL_FOCUS"
msgstr "Abs"
msgid "E_D_RUSSIAN_TWIST_DUMBBELL_EQUIP"
msgstr "Dumbbell"
msgid "E_D_RUSSIAN_TWIST_DUMBBELL_PREP_ONE"
msgstr "Sit on the floor and lean slightly backward with your knees bent and feet secured under the pads."
msgid "E_D_RUSSIAN_TWIST_DUMBBELL_PREP_TWO"
msgstr "Hold a dumbbell with both hands over your chest."
msgid "E_D_RUSSIAN_TWIST_DUMBBELL_EXEC_ONE"
msgstr "Exhale and squeeze your obliques to twist your torso to one side,"
msgid "E_D_RUSSIAN_TWIST_DUMBBELL_EXEC_TWO"
msgstr "Twist your torso back to the starting position."
msgid "E_D_RUSSIAN_TWIST_DUMBBELL_EXEC_THREE"
msgstr "Complete alternating between left and ight."
msgid "E_D_RUSSIAN_TWIST_DUMBBELL_TIP_ONE"
msgstr "Do not use hands or arms to assist the movement."
msgid "E_D_RUSSIAN_TWIST_DUMBBELL_TIP_TWO"
msgstr "Keep your legs stationary throughout the movement."
msgid "E_D_RUSSIAN_TWIST_DUMBBELL_TIP_THREE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_SEATED_CALF_PRESS_MACHINE_TITLE"
msgstr "Seated Calf Press: Machine"
msgid "E_D_SEATED_CALF_PRESS_MACHINE_FOCUS"
msgstr "Calves"
msgid "E_D_SEATED_CALF_PRESS_MACHINE_EQUIP"
msgstr "Leg Press Machine"
msgid "E_D_SEATED_CALF_PRESS_MACHINE_PREP_ONE"
msgstr "Sit on the machine and position our toes shoulder width apart at the bottom of the platform."
msgid "E_D_SEATED_CALF_PRESS_MACHINE_PREP_TWO"
msgstr "Hold handles on your sides."
msgid "E_D_SEATED_CALF_PRESS_MACHINE_EXEC_ONE"
msgstr "Exhale and press on the platform to extend your ankles as far as possible."
msgid "E_D_SEATED_CALF_PRESS_MACHINE_EXEC_TWO"
msgstr "Inhale, extend your calves to lower to the starting position slowly and repeat."
msgid "E_D_SEATED_CALF_PRESS_MACHINE_TIP_ONE"
msgstr "Do not lock our knees during the exercise."
msgid "E_D_SEATED_CALF_PRESS_MACHINE_TIP_TWO"
msgstr "Keep your legs and torso static during the exercise."
msgid "E_D_SEATED_CALF_RAISE_BARBELL_TITLE"
msgstr "Seated Calf Raise: Barbell"
msgid "E_D_SEATED_CALF_RAISE_BARBELL_FOCUS"
msgstr "Calves"
msgid "E_D_SEATED_CALF_RAISE_BARBELL_EQUIP"
msgstr "Barbell, Flat Bench"
msgid "E_D_SEATED_CALF_RAISE_BARBELL_PREP_ONE"
msgstr "Sit on the bench with feet together on the floor."
msgid "E_D_SEATED_CALF_RAISE_BARBELL_PREP_TWO"
msgstr "Hole a barbell and rest it on your thighs."
msgid "E_D_SEATED_CALF_RAISE_BARBELL_EXEC_ONE"
msgstr "Exhale and slowly raise our heels up as high as possible."
msgid "E_D_SEATED_CALF_RAISE_BARBELL_EXEC_TWO"
msgstr "Inhale, lower heels to the starting position slowly and repeat."
msgid "E_D_SEATED_CALF_RAISE_BARBELL_TIP_ONE"
msgstr "Keep your head, spine and neck in line and body balanced during the exercise."
msgid "E_D_SEATED_CALF_RAISE_BARBELL_TIP_TWO"
msgstr "Keep pressing the barbell against your knees during the exercise."
msgid "E_D_SEATED_CALF_RAISE_DUMBBELL_TITLE"
msgstr "Seated Calf Raise Dumbbell"
msgid "E_D_SEATED_CALF_RAISE_DUMBBELL_FOCUS"
msgstr "Calves"
msgid "E_D_SEATED_CALF_RAISE_DUMBBELL_EQUIP"
msgstr "Dumbbell, Flat Bench"
msgid "E_D_SEATED_CALF_RAISE_DUMBBELL_PREP_ONE"
msgstr "Sit on the bench and position you toes slightly apart on a weight plate."
msgid "E_D_SEATED_CALF_RAISE_DUMBBELL_PREP_TWO"
msgstr "Hold a dumbbell in each hand on your thighs and push your heels down."
msgid "E_D_SEATED_CALF_RAISE_DUMBBELL_EXEC_ONE"
msgstr "Exhale and slowly raise your heels up as high as possible."
msgid "E_D_SEATED_CALF_RAISE_DUMBBELL_EXEC_TWO"
msgstr "Inhale, lower heels to the starting position slowly and repeat."
msgid "E_D_SEATED_CALF_RAISE_DUMBBELL_TIP_ONE"
msgstr "Keep your head, spine and neck in line and body balanced during the exercise."
msgid "E_D_SEATED_CALF_RAISE_DUMBBELL_TIP_TWO"
msgstr "Keep pressing the dumbbells against your knees during the exercise."
msgid "E_D_SEATED_CALF_RAISE_MACHINE_TITLE"
msgstr "Seated Calf Raise: Machine"
msgid "E_D_SEATED_CALF_RAISE_MACHINE_FOCUS"
msgstr "Calves"
msgid "E_D_SEATED_CALF_RAISE_MACHINE_EQUIP"
msgstr "Calf Raise Machine"
msgid "E_D_SEATED_CALF_RAISE_MACHINE_PREP_ONE"
msgstr "Sit on the machine with toes slightly apart on the platforms and thighs under the pads."
msgid "E_D_SEATED_CALF_RAISE_MACHINE_PREP_TWO"
msgstr "Hold the handles in a neutral grip."
msgid "E_D_SEATED_CALF_RAISE_MACHINE_EXEC_ONE"
msgstr "Exhale and slowly raise your heels up as high as possible,"
msgid "E_D_SEATED_CALF_RAISE_MACHINE_EXEC_TWO"
msgstr "Inhale, lower heels to the starting position slowly and repeat."
msgid "E_D_SEATED_CALF_RAISE_MACHINE_TIP_ONE"
msgstr "Move through the ball of the feet rather than the toes."
msgid "E_D_SEATED_CHEST_FLY_MACHINE_TITLE"
msgstr "Seated Chest Fly: Machine"
msgid "E_D_SEATED_CHEST_FLY_MACHINE_FOCUS"
msgstr "Chest, Shoulders, Biceps"
msgid "E_D_SEATED_CHEST_FLY_MACHINE_EQUIP"
msgstr "Pec Deck Machine"
msgid "E_D_SEATED_CHEST_FLY_MACHINE_PREP_ONE"
msgstr "Sit on the machine with back supported and feet on the floor."
msgid "E_D_SEATED_CHEST_FLY_MACHINE_PREP_TWO"
msgstr "Hold the handles with palms facing forward."
msgid "E_D_SEATED_CHEST_FLY_MACHINE_EXEC_ONE"
msgstr "Exhale, squeeze your chest and press the handles together as close as possible."
msgid "E_D_SEATED_CHEST_FLY_MACHINE_EXEC_TWO"
msgstr "Pause for a few seconds, inhale and return to the starting position slowly and repeat."
msgid "E_D_SEATED_CHEST_FLY_MACHINE_TIP_ONE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_SEATED_CHEST_FLY_MACHINE_TIP_TWO"
msgstr "Keep your elbows slightly bent throughout the movement."
msgid "E_D_SEATED_CHEST_PRESS_MACHINE_TITLE"
msgstr "Seated Chest Press: Machine"
msgid "E_D_SEATED_CHEST_PRESS_MACHINE_FOCUS"
msgstr "Chest, Shoulders, Triceps"
msgid "E_D_SEATED_CHEST_PRESS_MACHINE_EQUIP"
msgstr "Chest Press Machine"
msgid "E_D_SEATED_CHEST_PRESS_MACHINE_PREP_ONE"
msgstr "Sit on the machine with feet on the floor."
msgid "E_D_SEATED_CHEST_PRESS_MACHINE_PREP_TWO"
msgstr "Bend your elbows and hold the handles at the height of your chest in an overhand grip."
msgid "E_D_SEATED_CHEST_PRESS_MACHINE_EXEC_ONE"
msgstr "Exhale and squeeze your chest to push the handles away from you to extend your arms straight."
msgid "E_D_SEATED_CHEST_PRESS_MACHINE_EXEC_TWO"
msgstr "Inhale and return to the starting position slowly and repeat."
msgid "E_D_SEATED_CHEST_PRESS_MACHINE_TIP_ONE"
msgstr "Keep the lever handles lower than your middle chest."
msgid "E_D_SEATED_CHEST_PRESS_MACHINE_TIP_TWO"
msgstr "Keep a straight back during the exercise,"
msgid "E_D_SEATED_CRUNCH_MACHINE_TITLE"
msgstr "Seated Crunch: Machine"
msgid "E_D_SEATED_CRUNCH_MACHINE_FOCUS"
msgstr "Abs"
msgid "E_D_SEATED_CRUNCH_MACHINE_EQUIP"
msgstr "Crunch Machine"
msgid "E_D_SEATED_CRUNCH_MACHINE_PREP_ONE"
msgstr "Sit on the machine with your back and elbows against the pads."
msgid "E_D_SEATED_CRUNCH_MACHINE_PREP_TWO"
msgstr "Secure your feet under the footpads and hold the handles."
msgid "E_D_SEATED_CRUNCH_MACHINE_EXEC_ONE"
msgstr "Exhale and crunch your abs to bring your torso. close to your legs."
msgid "E_D_SEATED_CRUNCH_MACHINE_EXEC_TWO"
msgstr "Pause at the bottom for a few seconds, inhale and return to the starting position slowly and repeat."
msgid "E_D_SEATED_CRUNCH_MACHINE_TIP_ONE"
msgstr "To add difficulty, add weights gradually."
msgid "E_D_SEATED_CRUNCH_MACHINE_TIP_TWO"
msgstr "Keep a braced core during the exercise."
msgid "E_D_SEATED_HIP_ABDUCTOR_MACHINE_TITLE"
msgstr "Seated Hip Abductor : Machine"
msgid "E_D_SEATED_HIP_ABDUCTOR_MACHINE_FOCUS"
msgstr "Glutes, Abductors"
msgid "E_D_SEATED_HIP_ABDUCTOR_MACHINE_EQUIP"
msgstr "Seated Hip Machine"
msgid "E_D_SEATED_HIP_ABDUCTOR_MACHINE_PREP_ONE"
msgstr "Sit on the machine with your feet on the platform ar adjust the side pads until your legs touch each other:"
msgid "E_D_SEATED_HIP_ABDUCTOR_MACHINE_PREP_TWO"
msgstr "Hold the handles on your sides."
msgid "E_D_SEATED_HIP_ABDUCTOR_MACHINE_EXEC_ONE"
msgstr "Exhale and open your legs out to the sides as far as possible."
msgid "E_D_SEATED_HIP_ABDUCTOR_MACHINE_EXEC_TWO"
msgstr "Inhale and return to the starting position slowly and repeat."
msgid "E_D_SEATED_HIP_ABDUCTOR_MACHINE_TIP_ONE"
msgstr "Do not let your legs touch each other throughout the motion:"
msgid "E_D_SEATED_HIP_ABDUCTOR_MACHINE_TIP_TWO"
msgstr "Keep your back against the pad during the exercise."
msgid "E_D_SEATED_HIP_ADDUCTOR_MACHINE_TITLE"
msgstr "Seated Hip Adductor Machine"
msgid "E_D_SEATED_HIP_ADDUCTOR_MACHINE_FOCUS"
msgstr "Glutes, Adductors"
msgid "E_D_SEATED_HIP_ADDUCTOR_MACHINE_EQUIP"
msgstr "Seated Hip Machine"
msgid "E_D_SEATED_HIP_ADDUCTOR_MACHINE_PREP_ONE"
msgstr "Sit on the machine with your feet on the platform and adjust the side pads until your legs are fully open."
msgid "E_D_SEATED_HIP_ADDUCTOR_MACHINE_PREP_TWO"
msgstr "Hold the handles on your sides."
msgid "E_D_SEATED_HIP_ADDUCTOR_MACHINE_EXEC_ONE"
msgstr "Exhale and press your legs together as close as possible,"
msgid "E_D_SEATED_HIP_ADDUCTOR_MACHINE_EXEC_TWO"
msgstr "Inhale and return to the starting position slowly and repeat."
msgid "E_D_SEATED_HIP_ADDUCTOR_MACHINE_TIP_ONE"
msgstr "Keep your back against the pad during the exercise."
msgid "E_D_SEATED_HIP_EXTERNAL_ROTATION_BAND_TITLE"
msgstr "Seated Hip External Rotation: Band"
msgid "E_D_SEATED_HIP_EXTERNAL_ROTATION_BAND_FOCUS"
msgstr "Glutes, Adductors"
msgid "E_D_SEATED_HIP_EXTERNAL_ROTATION_BAND_EQUIP"
msgstr "Handle Band, Flat Bench"
msgid "E_D_SEATED_HIP_EXTERNAL_ROTATION_BAND_PREP_ONE"
msgstr "Attach one end of the band to an object and the other end around your ankle."
msgid "E_D_SEATED_HIP_EXTERNAL_ROTATION_BAND_PREP_TWO"
msgstr "Sit on the bench sideways to the object."
msgid "E_D_SEATED_HIP_EXTERNAL_ROTATION_BAND_EXEC_ONE"
msgstr "Rotate your working leg inward slowly."
msgid "E_D_SEATED_HIP_EXTERNAL_ROTATION_BAND_EXEC_TWO"
msgstr "Slowly return to the starting position and repeat."
msgid "E_D_SEATED_HIP_EXTERNAL_ROTATION_BAND_EXEC_THREE"
msgstr "Switch to the other side and repeat the same repetitions."
msgid "E_D_SEATED_HIP_EXTERNAL_ROTATION_BAND_TIP_ONE"
msgstr "Keep your knees pointing forward during the exercise."
msgid "E_D_SEATED_HIP_INTERNAL_ROTATION_BAND_TITLE"
msgstr "Seated Hip Internal Rotation: Band"
msgid "E_D_SEATED_HIP_INTERNAL_ROTATION_BAND_FOCUS"
msgstr "Glutes, Abductors"
msgid "E_D_SEATED_HIP_INTERNAL_ROTATION_BAND_EQUIP"
msgstr "Handle Band, Flat Bench"
msgid "E_D_SEATED_HIP_INTERNAL_ROTATION_BAND_PREP_ONE"
msgstr "Attach one end of the band to an object and the other end around your ankle."
msgid "E_D_SEATED_HIP_INTERNAL_ROTATION_BAND_PREP_TWO"
msgstr "Sit on the bench sideways to the object."
msgid "E_D_SEATED_HIP_INTERNAL_ROTATION_BAND_EXEC_ONE"
msgstr "Rotate your working leg outward slowly."
msgid "E_D_SEATED_HIP_INTERNAL_ROTATION_BAND_EXEC_TWO"
msgstr "lowly return to the starting position and repeat."
msgid "E_D_SEATED_HIP_INTERNAL_ROTATION_BAND_EXEC_THREE"
msgstr "Switch to the other side and repeat the same repetitions."
msgid "E_D_SEATED_HIP_INTERNAL_ROTATION_BAND_TIP_ONE"
msgstr "Keep your knees pointing forward during the exercise."
msgid "E_D_SEATED_IN_AND_OUT_TITLE"
msgstr "Seated In-And-Out"
msgid "E_D_SEATED_IN_AND_OUT_FOCUS"
msgstr "Abs"
msgid "E_D_SEATED_IN_AND_OUT_EQUIP"
msgstr "Bodyweight"
msgid "E_D_SEATED_IN_AND_OUT_PREP_ONE"
msgstr "Sit on the floor and lean backward with your legs extended together in front and slightly lifted and hands at the sides of your hips to stabilize your body."
msgid "E_D_SEATED_IN_AND_OUT_EXEC_ONE"
msgstr "Squeeze your abs and bend your knees to bring your torso and legs close to each other as much as possible."
msgid "E_D_SEATED_IN_AND_OUT_EXEC_TWO"
msgstr "Inhale and return to the starting position slowly and repeat."
msgid "E_D_SEATED_IN_AND_OUT_TIP_ONE"
msgstr "Do not let your legs touch the floor throughout the movement."
msgid "E_D_SEATED_IN_AND_OUT_TIP_TWO"
msgstr "To add difficulty, use ankle weights."
msgid "E_D_SEATED_IN_AND_OUT_TIP_THREE"
msgstr "Keep a braced core during the exercise."
msgid "E_D_SEATED_IN_AND_OUT_DUMBBELL_TITLE"
msgstr "Seated In-And-Out: Dumbbell"
msgid "E_D_SEATED_IN_AND_OUT_DUMBBELL_FOCUS"
msgstr "Abs"
msgid "E_D_SEATED_IN_AND_OUT_DUMBBELL_EQUIP"
msgstr "Dumbbell"
msgid "E_D_SEATED_IN_AND_OUT_DUMBBELL_PREP_ONE"
msgstr "Sit on the floor with your legs extended in front and hold a dumbbell with your ankles."
msgid "E_D_SEATED_IN_AND_OUT_DUMBBELL_PREP_TWO"
msgstr "Lean backward with hands at the sides of your hips to stabilize your body."
msgid "E_D_SEATED_IN_AND_OUT_DUMBBELL_EXEC_ONE"
msgstr "Squeeze your abs and bend your knees to bring your torso and legs close to each other as much as possible."
msgid "E_D_SEATED_IN_AND_OUT_DUMBBELL_EXEC_TWO"
msgstr "Return to the starting position slowly and repeat."
msgid "E_D_SEATED_IN_AND_OUT_DUMBBELL_TIP_ONE"
msgstr "Do not let your legs touch the floor throughout the movement."
msgid "E_D_SEATED_IN_AND_OUT_DUMBBELL_TIP_TWO"
msgstr "Keep a braced core during the exercise."
msgid "E_D_SEATED_LEG_CURL_MACHINE_TITLE"
msgstr "Seated Leg Curl: Machine"
msgid "E_D_SEATED_LEG_CURL_MACHINE_FOCUS"
msgstr "Hamstrings, Calves"
msgid "E_D_SEATED_LEG_CURL_MACHINE_EQUIP"
msgstr "Leg Curl Machine"
msgid "E_D_SEATED_LEG_CURL_MACHINE_PREP_ONE"
msgstr "Sit on the machine and position your ankles just above the pads."
msgid "E_D_SEATED_LEG_CURL_MACHINE_PREP_TWO"
msgstr "Hold the handles in a neutral grip."
msgid "E_D_SEATED_LEG_CURL_MACHINE_EXEC_ONE"
msgstr "Squeeze your hamstrings to curl your legs and pull the pads back until your legs bend at 90-degree angle."
msgid "E_D_SEATED_LEG_CURL_MACHINE_EXEC_TWO"
msgstr "Return to the starting position and repeat."
msgid "E_D_SEATED_LEG_CURL_MACHINE_TIP_ONE"
msgstr "Do not lock out your knees at the top."
msgid "E_D_SEATED_LEG_PRESS_MACHINE_TITLE"
msgstr "Seated Leg Press: Machine"
msgid "E_D_SEATED_LEG_PRESS_MACHINE_FOCUS"
msgstr "Glutes, Quadriceps, Hamstrings"
msgid "E_D_SEATED_LEG_PRESS_MACHINE_EQUIP"
msgstr "Leg Press Machine"
msgid "E_D_SEATED_LEG_PRESS_MACHINE_PREP_ONE"
msgstr "Sit on the machine with knees bent and feet on the platform shoulder width apart."
msgid "E_D_SEATED_LEG_PRESS_MACHINE_PREP_TWO"
msgstr "Hold the handles on. your sides."
msgid "E_D_SEATED_LEG_PRESS_MACHINE_EXEC_ONE"
msgstr "Press the platform away from you through your, heels until your legs are almost fully extended."
msgid "E_D_SEATED_LEG_PRESS_MACHINE_EXEC_TWO"
msgstr "Bring the knees back to the starting position and repeat."
msgid "E_D_SEATED_LEG_PRESS_MACHINE_TIP_ONE"
msgstr "Keep your hips steady during the exercise."
msgid "E_D_SEATED_LEG_PRESS_MACHINE_TIP_TWO"
msgstr "Do not lock out your knees at the top."
msgid "E_D_SEATED_LEG_PRESS_MACHINE_TIP_THREE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_SEATED_OVERHEAD_PRESS_BARBELL_TITLE"
msgstr "Seated Overhead Press: Barbell"
msgid "E_D_SEATED_OVERHEAD_PRESS_BARBELL_FOCUS"
msgstr "Shoulders"
msgid "E_D_SEATED_OVERHEAD_PRESS_BARBELL_EQUIP"
msgstr "Barbell, Flat Bench"
msgid "E_D_SEATED_OVERHEAD_PRESS_BARBELL_PREP_ONE"
msgstr "Sit on a bench with feet on the floor."
msgid "E_D_SEATED_OVERHEAD_PRESS_BARBELL_PREP_TWO"
msgstr "Hold the barbell in front of your shoulders with hands slightly wider than shoulder width apart and palms facing forward."
msgid "E_D_SEATED_OVERHEAD_PRESS_BARBELL_EXEC_ONE"
msgstr "Exhale and squeeze your shoulders to lift the bar vertically until your arms are straight overhead"
msgid "E_D_SEATED_OVERHEAD_PRESS_BARBELL_EXEC_TWO"
msgstr "Pause for a few seconds, inhale, return to the starting position slowly and repeat."
msgid "E_D_SEATED_OVERHEAD_PRESS_BARBELL_TIP_ONE"
msgstr "Do not lock out your elbows at the top."
msgid "E_D_SEATED_OVERHEAD_PRESS_BARBELL_TIP_TWO"
msgstr "Do not rest the barbell on your shoulders."
msgid "E_D_SEATED_OVERHEAD_PRESS_BARBELL_TIP_THREE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_SEATED_OVERHEAD_PRESS_DUMBBELL_TITLE"
msgstr "Seated Overhead Press: Dumbbell"
msgid "E_D_SEATED_OVERHEAD_PRESS_DUMBBELL_FOCUS"
msgstr "Shoulders"
msgid "E_D_SEATED_OVERHEAD_PRESS_DUMBBELL_EQUIP"
msgstr "Dumbbell, Flat Bench"
msgid "E_D_SEATED_OVERHEAD_PRESS_DUMBBELL_PREP_ONE"
msgstr "Sit on a bench with feet on the floor."
msgid "E_D_SEATED_OVERHEAD_PRESS_DUMBBELL_PREP_TWO"
msgstr "Hold a dumbbell in each hand at your sides with your elbows bent at 90 degrees and palms facing forward."
msgid "E_D_SEATED_OVERHEAD_PRESS_DUMBBELL_EXEC_ONE"
msgstr "Exhale and squeeze your shoulders to lift dumbbells vertically until your arms are straight overhead."
msgid "E_D_SEATED_OVERHEAD_PRESS_DUMBBELL_EXEC_TWO"
msgstr "Pause for a few seconds, inhale, return to the starting position slowly and repeat."
msgid "E_D_SEATED_OVERHEAD_PRESS_DUMBBELL_TIP_ONE"
msgstr "Do not lock out your elbows at the top."
msgid "E_D_SEATED_OVERHEAD_PRESS_DUMBBELL_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_SEATED_OVERHEAD_PRESS_MACHINE_TITLE"
msgstr "Seated Overhead Press: Machine"
msgid "E_D_SEATED_OVERHEAD_PRESS_MACHINE_FOCUS"
msgstr "Shoulders"
msgid "E_D_SEATED_OVERHEAD_PRESS_MACHINE_EQUIP"
msgstr "Shoulder Press Machine"
msgid "E_D_SEATED_OVERHEAD_PRESS_MACHINE_PREP_ONE"
msgstr "Sit on the machine with feet on the floor."
msgid "E_D_SEATED_OVERHEAD_PRESS_MACHINE_PREP_TWO"
msgstr "Hold a. handle in each hand at your sides with your elbows bent at 90 degrees and palms facing forward."
msgid "E_D_SEATED_OVERHEAD_PRESS_MACHINE_EXEC_ONE"
msgstr "Exhale and squeeze your shoulders to lift the bar vertically until your arms are straight overhead."
msgid "E_D_SEATED_OVERHEAD_PRESS_MACHINE_EXEC_TWO"
msgstr "Pause for a few seconds, inhale, return to the starting position slowly and repeat."
msgid "E_D_SEATED_OVERHEAD_PRESS_MACHINE_TIP_ONE"
msgstr "Do not lock out your elbows at the top."
msgid "E_D_SEATED_OVERHEAD_PRESS_MACHINE_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_SEATED_OVERHEAD_PRESS_SMITH_MACHINE_TITLE"
msgstr "Seated Overhead Press: Smith Machine"
msgid "E_D_SEATED_OVERHEAD_PRESS_SMITH_MACHINE_FOCUS"
msgstr "Shoulders"
msgid "E_D_SEATED_OVERHEAD_PRESS_SMITH_MACHINE_EQUIP"
msgstr "Smith Machine, Flat Bench"
msgid "E_D_SEATED_OVERHEAD_PRESS_SMITH_MACHINE_PREP_ONE"
msgstr "Sit on a bench with feet on the floor."
msgid "E_D_SEATED_OVERHEAD_PRESS_SMITH_MACHINE_PREP_TWO"
msgstr "Hold the barbell in front of your shoulders with hands slightly wider than shoulder width apart and palms facing forward."
msgid "E_D_SEATED_OVERHEAD_PRESS_SMITH_MACHINE_EXEC_ONE"
msgstr "Exhale and squeeze your shoulders to lift the bar vertically until your arms are straight overhead"
msgid "E_D_SEATED_OVERHEAD_PRESS_SMITH_MACHINE_EXEC_TWO"
msgstr "Pause for a few seconds, inhale, return to the starting position slowly and repeat."
msgid "E_D_SEATED_OVERHEAD_PRESS_SMITH_MACHINE_TIP_ONE"
msgstr "Do not lock out your elbows at the top."
msgid "E_D_SEATED_OVERHEAD_PRESS_SMITH_MACHINE_TIP_TWO"
msgstr "Do not rest the barbell on your shoulders."
msgid "E_D_SEATED_OVERHEAD_PRESS_SMITH_MACHINE_TIP_THREE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_SEATED_REAR_DELT_ROW_DUMBBELL_TITLE"
msgstr "Seated Rear Delt Row: Dumbbell"
msgid "E_D_SEATED_REAR_DELT_ROW_DUMBBELL_FOCUS"
msgstr "Shoulders, Traps"
msgid "E_D_SEATED_REAR_DELT_ROW_DUMBBELL_EQUIP"
msgstr "Dumbbell, Flat Bench"
msgid "E_D_SEATED_REAR_DELT_ROW_DUMBBELL_PREP_ONE"
msgstr "Sit on the bench with feet together on the floor."
msgid "E_D_SEATED_REAR_DELT_ROW_DUMBBELL_PREP_TWO"
msgstr "Bent at your hips to hold a dumbbell in each hand with palms facing backwards and arms stretched."
msgid "E_D_SEATED_REAR_DELT_ROW_DUMBBELL_EXEC_ONE"
msgstr "Squeeze your shoulder blades and bend the elbows to bring the dumbbells upwards to your chest."
msgid "E_D_SEATED_REAR_DELT_ROW_DUMBBELL_EXEC_TWO"
msgstr "Return to the starting position slowly and repeat."
msgid "E_D_SEATED_REAR_DELT_ROW_DUMBBELL_TIP_ONE"
msgstr "Move the dumbbells vertically."
msgid "E_D_SEATED_REAR_DELT_ROW_DUMBBELL_TIP_TWO"
msgstr "Keep a straight back during the exercise."
msgid "E_D_SEATED_REVERSE_FLY_MACHINE_TITLE"
msgstr "Seated Reverse Fly: Machine"
msgid "E_D_SEATED_REVERSE_FLY_MACHINE_FOCUS"
msgstr "Shoulders, Traps"
msgid "E_D_SEATED_REVERSE_FLY_MACHINE_EQUIP"
msgstr "Pec Deck Machine"
msgid "E_D_SEATED_REVERSE_FLY_MACHINE_PREP_ONE"
msgstr "Sit on the machine with feet on the floor."
msgid "E_D_SEATED_REVERSE_FLY_MACHINE_PREP_TWO"
msgstr "Hold the handles in front of you with arms straight at shoulder level."
msgid "E_D_SEATED_REVERSE_FLY_MACHINE_EXEC_ONE"
msgstr "Exhale and squeeze your shoulder blades together to pull your arms out to your sides."
msgid "E_D_SEATED_REVERSE_FLY_MACHINE_EXEC_TWO"
msgstr "Pause for a few seconds when your arms are parallel to your shoulders."
msgid "E_D_SEATED_REVERSE_FLY_MACHINE_EXEC_THREE"
msgstr "Inhale and return to the starting position and repeat."
msgid "E_D_SEATED_REVERSE_FLY_MACHINE_TIP_ONE"
msgstr "Do not allow the elbows to shift behind the body."
msgid "E_D_SEATED_REVERSE_FLY_MACHINE_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
