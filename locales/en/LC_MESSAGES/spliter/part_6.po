msgid "E_D_FRONT_SQUAT_CABLE_TITLE"
msgstr "Front Squat: Cable"
msgid "E_D_FRONT_SQUAT_CABLE_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_FRONT_SQUAT_CABLE_PREP_TWO"
msgstr "Hold the bar at shoulder height with arms bent at your sides."
msgid "E_D_FRONT_SQUAT_CABLE_EXEC_ONE"
msgstr "Inhale and squat down until your thighs are parallel to the floor."
msgid "E_D_FRONT_SQUAT_CABLE_EXEC_TWO"
msgstr "Hold for a few seconds to feel the contraction."
msgid "E_D_FRONT_SQUAT_CABLE_EXEC_THREE"
msgstr "Exhale, drive through the heels and midfoot to the starting position slowly and repeat."
msgid "E_D_FRONT_SQUAT_CABLE_TIP_ONE"
msgstr "Keep the tiptoes in the same direction as your knees."
msgid "E_D_FRONT_SQUAT_CABLE_TIP_TWO"
msgstr "Do not let your knees go over your toes."
msgid "E_D_FRONT_SQUAT_CABLE_TIP_THREE"
msgstr "Do not lock out your knees at the top."
msgid "E_D_FRONT_SQUAT_CABLE_TIP_FOUR"
msgstr "Keep your glutes, quadriceps and hamstrings engaged"
msgid "E_D_FRONT_SQUAT_DUMBBELL_TITLE"
msgstr "Front Squat- Dumbbell"
msgid "E_D_FRONT_SQUAT_DUMBBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_FRONT_SQUAT_DUMBBELL_PREP_TWO"
msgstr "Hold dumbbell in each hand in a neutral grip at chin height with arms bent at your sides."
msgid "E_D_FRONT_SQUAT_DUMBBELL_EXEC_ONE"
msgstr "Inhale and squat down until your thighs are parallel to the floor."
msgid "E_D_FRONT_SQUAT_DUMBBELL_EXEC_TWO"
msgstr "Hold for a few seconds to feel the contraction."
msgid "E_D_FRONT_SQUAT_DUMBBELL_EXEC_THREE"
msgstr "Exhale, drive through the heels and midfoot to the starting position slowly and repeat."
msgid "E_D_FRONT_SQUAT_DUMBBELL_TIP_ONE"
msgstr "Keep the tiptoes in the same direction as your knees."
msgid "E_D_FRONT_SQUAT_DUMBBELL_TIP_TWO"
msgstr "Do not let your knees go over your toes."
msgid "E_D_FRONT_SQUAT_DUMBBELL_TIP_THREE"
msgstr "Do not lock out your knees at the top."
msgid "E_D_FRONT_SQUAT_DUMBBELL_TIP_FOUR"
msgstr "Keep your glutes, quadriceps and hamstrings engaged."
msgid "E_D_FRONT_SQUAT_KETTLEBELL_TITLE"
msgstr "Front Squat: Kettlebell"
msgid "E_D_FRONT_SQUAT_KETTLEBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_FRONT_SQUAT_KETTLEBELL_PREP_TWO"
msgstr "Hold a kettlebell with both hands at shoulder height and arms bent at your sides."
msgid "E_D_FRONT_SQUAT_KETTLEBELL_EXEC_ONE"
msgstr "Inhale and squat down until your thighs are parallel to the floor."
msgid "E_D_FRONT_SQUAT_KETTLEBELL_EXEC_TWO"
msgstr "Hold for a few seconds to feel the contraction."
msgid "E_D_FRONT_SQUAT_KETTLEBELL_EXEC_THREE"
msgstr "Exhale, drive through the heels and midfoot to the starting position slowly and repeat."
msgid "E_D_FRONT_SQUAT_KETTLEBELL_TIP_ONE"
msgstr "Keep the tiptoes in the same direction as your knees."
msgid "E_D_FRONT_SQUAT_KETTLEBELL_TIP_TWO"
msgstr "Do not let your knees go over your toes."
msgid "E_D_FRONT_SQUAT_KETTLEBELL_TIP_THREE"
msgstr "Do not lock out your knees at the top."
msgid "E_D_FRONT_SQUAT_KETTLEBELL_TIP_FOUR"
msgstr "Keep your glutes, quadriceps and hamstrings engaged"
msgid "E_D_FRONT_SQUAT_SMITH_MACHINE_TITLE"
msgstr "Front Squat: Smith Machine"
msgid "E_D_FRONT_SQUAT_SMITH_MACHINE_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_FRONT_SQUAT_SMITH_MACHINE_PREP_TWO"
msgstr "Position the bar across your front deltoids and hold it with your arms crossed horizontally."
msgid "E_D_FRONT_SQUAT_SMITH_MACHINE_EXEC_ONE"
msgstr "Inhale and squat down until your thighs are parallel to the floor."
msgid "E_D_FRONT_SQUAT_SMITH_MACHINE_EXEC_TWO"
msgstr "Hold for a few seconds to feel the contraction."
msgid "E_D_FRONT_SQUAT_SMITH_MACHINE_EXEC_THREE"
msgstr "Exhale, drive through the heels and midfoot to the starting position slowly and repeat."
msgid "E_D_FRONT_SQUAT_SMITH_MACHINE_TIP_ONE"
msgstr "Keep the tiptoes in the same direction as your knees."
msgid "E_D_FRONT_SQUAT_SMITH_MACHINE_TIP_TWO"
msgstr "Do not let your knees go over your toes."
msgid "E_D_FRONT_SQUAT_SMITH_MACHINE_TIP_THREE"
msgstr "Do not lock out your knees at the top."
msgid "E_D_FRONT_SQUAT_SMITH_MACHINE_TIP_FOUR"
msgstr "Keep your glutes, quadriceps and hamstrings engaged."
msgid "E_D_GLUTE_BRIDGE_BODYWEIGHT_TITLE"
msgstr "Glute Bridge"
msgid "E_D_GLUTE_BRIDGE_BODYWEIGHT_PREP_ONE"
msgstr "Lie with knees bent and feet on the floor."
msgid "E_D_GLUTE_BRIDGE_BODYWEIGHT_PREP_TWO"
msgstr "Keep your arms extended by your sides with palms down."
msgid "E_D_GLUTE_BRIDGE_BODYWEIGHT_EXEC_ONE"
msgstr "Press your heels to lift hips up until your body forms a straight line from your shoulders to your knees."
msgid "E_D_GLUTE_BRIDGE_BODYWEIGHT_EXEC_TWO"
msgstr "Hold for a few seconds at the top position."
msgid "E_D_GLUTE_BRIDGE_BODYWEIGHT_EXEC_THREE"
msgstr "Lower back to the starting position and repeat."
msgid "E_D_GLUTE_BRIDGE_BODYWEIGHT_TIP_ONE"
msgstr "Do not allow your knees fall inward or outward."
msgid "E_D_GLUTE_BRIDGE_BODYWEIGHT_TIP_TWO"
msgstr "To add difficulty, add weights to your hips."
msgid "E_D_GLUTE_BRIDGE_BODYWEIGHT_TIP_THREE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_GLUTE_BRIDGE_ABDUCTION_TITLE"
msgstr "Glute Bridge Abduction"
msgid "E_D_GLUTE_BRIDGE_ABDUCTION_PREP_ONE"
msgstr "Lie with knees bent and feet on the floor."
msgid "E_D_GLUTE_BRIDGE_ABDUCTION_PREP_TWO"
msgstr "Raise your hips until your body forms a straight line from your shoulders to your knees."
msgid "E_D_GLUTE_BRIDGE_ABDUCTION_EXEC_ONE"
msgstr "Squeeze as if there is a ball between your knees and hold for a few seconds."
msgid "E_D_GLUTE_BRIDGE_ABDUCTION_EXEC_TWO"
msgstr "Return to the starting position and repeat."
msgid "E_D_GLUTE_BRIDGE_ABDUCTION_TIP_ONE"
msgstr "Do not push your hips too high or sag your hips."
msgid "E_D_GLUTE_BRIDGE_ABDUCTION_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_GLUTE_BRIDGE_FEET_ON_BENCH_TITLE"
msgstr "Glute Bridge Feet On Bench"
msgid "E_D_GLUTE_BRIDGE_FEET_ON_BENCH_PREP_ONE"
msgstr "Lie with knees bent and heels on the bench."
msgid "E_D_GLUTE_BRIDGE_FEET_ON_BENCH_PREP_TWO"
msgstr "Keep your arms extended by your sides with palms down."
msgid "E_D_GLUTE_BRIDGE_FEET_ON_BENCH_EXEC_ONE"
msgstr "Press your heels to lift hips up until your body forms a straight line from your shoulders to your knees."
msgid "E_D_GLUTE_BRIDGE_FEET_ON_BENCH_EXEC_TWO"
msgstr "Hold for a few seconds at the top position."
msgid "E_D_GLUTE_BRIDGE_FEET_ON_BENCH_EXEC_THREE"
msgstr "Lower back to the starting position and repeat."
msgid "E_D_GLUTE_BRIDGE_FEET_ON_BENCH_TIP_ONE"
msgstr "Do not allow your knees fall inward or outward."
msgid "E_D_GLUTE_BRIDGE_FEET_ON_BENCH_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_GLUTE_BRIDGE_FEET_ON_BENCH_BARBELL_TITLE"
msgstr "Glute Bridge Feet On Bench: Barbell"
msgid "E_D_GLUTE_BRIDGE_FEET_ON_BENCH_BARBELL_PREP_ONE"
msgstr "Lie with knees bent and heels on the bench."
msgid "E_D_GLUTE_BRIDGE_FEET_ON_BENCH_BARBELL_PREP_TWO"
msgstr "Hold bar in a wide overhand grip and place it over the crease of your hips."
msgid "E_D_GLUTE_BRIDGE_FEET_ON_BENCH_BARBELL_EXEC_ONE"
msgstr "Press your heels to lift hips up until your body forms a straight line from your shoulders to your knees."
msgid "E_D_GLUTE_BRIDGE_FEET_ON_BENCH_BARBELL_EXEC_TWO"
msgstr "Hold for a few seconds at the top position."
msgid "E_D_GLUTE_BRIDGE_FEET_ON_BENCH_BARBELL_EXEC_THREE"
msgstr "Lower back to the starting position and repeat."
msgid "E_D_GLUTE_BRIDGE_FEET_ON_BENCH_BARBELL_TIP_ONE"
msgstr "Do not allow your knees fall inward or outward."
msgid "E_D_GLUTE_BRIDGE_FEET_ON_BENCH_BARBELL_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_GLUTE_BRIDGE_BARBELL_TITLE"
msgstr "Glute Bridge: Barbell"
msgid "E_D_GLUTE_BRIDGE_BARBELL_PREP_ONE"
msgstr "Lie with knees bent and feet on the floor."
msgid "E_D_GLUTE_BRIDGE_BARBELL_PREP_TWO"
msgstr "Hold a bar in a wide overhand grip and place it over the crease of your hips."
msgid "E_D_GLUTE_BRIDGE_BARBELL_EXEC_ONE"
msgstr "Press your heels to lift hips up until your body forms a straight line from your shoulders to your knees."
msgid "E_D_GLUTE_BRIDGE_BARBELL_EXEC_TWO"
msgstr "Hold for a few seconds at the top position."
msgid "E_D_GLUTE_BRIDGE_BARBELL_EXEC_THREE"
msgstr "Lower back to the starting position and repeat."
msgid "E_D_GLUTE_BRIDGE_BARBELL_TIP_ONE"
msgstr "Do not allow your knees fall inward or outward."
msgid "E_D_GLUTE_BRIDGE_BARBELL_TIP_TWO"
msgstr "Squeeze your glutes when lifting your hips."
msgid "E_D_GLUTE_BRIDGE_BARBELL_TIP_THREE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_GLUTE_BRIDGE_DUMBBELL_TITLE"
msgstr "Glute Bridge - Dumbbell"
msgid "E_D_GLUTE_BRIDGE_DUMBBELL_PREP_ONE"
msgstr "Lie with knees bent and feet together on the floor."
msgid "E_D_GLUTE_BRIDGE_DUMBBELL_PREP_TWO"
msgstr "Hold a dumbbell with both hands and place it over the crease of your hips."
msgid "E_D_GLUTE_BRIDGE_DUMBBELL_EXEC_ONE"
msgstr "Press your heels to lift hips up until your body forms a straight line from your shoulders to your knees."
msgid "E_D_GLUTE_BRIDGE_DUMBBELL_EXEC_TWO"
msgstr "Hold for a few seconds at the top position."
msgid "E_D_GLUTE_BRIDGE_DUMBBELL_EXEC_THREE"
msgstr "Lower back to the starting position and repeat."
msgid "E_D_GLUTE_BRIDGE_DUMBBELL_TIP_ONE"
msgstr "Do not allow your knees fall inward or outward."
msgid "E_D_GLUTE_BRIDGE_DUMBBELL_TIP_TWO"
msgstr "Squeeze your glutes when lifting your hips."
msgid "E_D_GLUTE_BRIDGE_DUMBBELL_TIP_THREE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_GLUTE_FROG_BRIDGE_TITLE"
msgstr "Glute Frog Bridge"
msgid "E_D_GLUTE_FROG_BRIDGE_PREP_ONE"
msgstr "Lie with the soles of your feet together and knees spreading out."
msgid "E_D_GLUTE_FROG_BRIDGE_PREP_TWO"
msgstr "Place upper arms by your sides on the floor to support you."
msgid "E_D_GLUTE_FROG_BRIDGE_EXEC_ONE"
msgstr "Squeeze your abs and glutes to lift hips up towards the ceiling"
msgid "E_D_GLUTE_FROG_BRIDGE_EXEC_TWO"
msgstr "Hold for a few seconds at the top position."
msgid "E_D_GLUTE_FROG_BRIDGE_EXEC_THREE"
msgstr "Slowly return to the starting position and repeat."
msgid "E_D_GLUTE_FROG_BRIDGE_TIP_ONE"
msgstr "Do not allow your knees fall inward."
msgid "E_D_GLUTE_FROG_BRIDGE_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_GLUTE_HAM_RAISE_TITLE"
msgstr "Glute Ham Raise"
msgid "E_D_GLUTE_HAM_RAISE_FOCUS"
msgstr "Glutes, Hamstrings"
msgid "E_D_GLUTE_HAM_RAISE_EQUIP"
msgstr "Bodyweight"
msgid "E_D_GLUTE_HAM_RAISE_PREP_ONE"
msgstr "Kneel down on the bench with your body perpendicular to it."
msgid "E_D_GLUTE_HAM_RAISE_PREP_TWO"
msgstr "Secure your feet under the footpads and cross your arms in front of your chest."
msgid "E_D_GLUTE_HAM_RAISE_EXEC_ONE"
msgstr "Exhale and lower your torso forward by straightening your knees until your body is horizontal."
msgid "E_D_GLUTE_HAM_RAISE_EXEC_TWO"
msgstr "Inhale and squeeze your hips to return to the starting position slowly and repeat."
msgid "E_D_GLUTE_HAM_RAISE_TIP_ONE"
msgstr "To add difficulty, hold a weight plate across your chest or behind your neck"
msgid "E_D_GLUTE_HAM_RAISE_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_GLUTE_KICKBACK_TITLE"
msgstr "Glute Kickback"
msgid "E_D_GLUTE_KICKBACK_FOCUS"
msgstr "Glutes, Hamstrings"
msgid "E_D_GLUTE_KICKBACK_EQUIP"
msgstr "Bodyweight"
msgid "E_D_GLUTE_KICKBACK_PREP_ONE"
msgstr "Place your one bent leg and both hands under your shoulders on the bench."
msgid "E_D_GLUTE_KICKBACK_PREP_TWO"
msgstr "Rest the other leg beside the bench."
msgid "E_D_GLUTE_KICKBACK_EXEC_ONE"
msgstr "Stretch and kick the free leg back until your thigh is parallel with the floor."
msgid "E_D_GLUTE_KICKBACK_EXEC_TWO"
msgstr "Squeeze your glutes and hold for a few seconds at the top position."
msgid "E_D_GLUTE_KICKBACK_EXEC_THREE"
msgstr "Return to the starting position and repeat."
msgid "E_D_GLUTE_KICKBACK_EXEC_FOUR"
msgstr "Switch to the other side and repeat the same repetitions."
msgid "E_D_GLUTE_KICKBACK_TIP_ONE"
msgstr "Do not let your leg touch the floor when lowering it."
msgid "E_D_GLUTE_KICKBACK_TIP_TWO"
msgstr "Keep your pelvis staying pointed toward the ground."
msgid "E_D_GLUTE_KICKBACK_TIP_THREE"
msgstr "Keep your torso steady with a straight back and braced core during the exercise."
msgid "E_D_GLUTE_KICKBACK_TIP_FOUR"
msgstr "To add difficulty, use ankle weights."
msgid "E_D_GLUTE_KICKBACK_BAND_TITLE"
msgstr "Glute Kickback: Band"
msgid "E_D_GLUTE_KICKBACK_BAND_FOCUS"
msgstr "Glutes, Hamstrings"
msgid "E_D_GLUTE_KICKBACK_BAND_EQUIP"
msgstr "Handle Band"
msgid "E_D_GLUTE_KICKBACK_BAND_PREP_ONE"
msgstr "Place the band under one midfoot."
msgid "E_D_GLUTE_KICKBACK_BAND_PREP_TWO"
msgstr "Begin on all four with knees hip-width apart and hands holding the handles under your shoulders."
msgid "E_D_GLUTE_KICKBACK_BAND_EXEC_ONE"
msgstr "Stretch and kick one leg back until your thigh is parallel with the floor."
msgid "E_D_GLUTE_KICKBACK_BAND_EXEC_TWO"
msgstr "Squeeze your glutes and hold for a few seconds at the top position."
msgid "E_D_GLUTE_KICKBACK_BAND_EXEC_THREE"
msgstr "Return to the starting position slowly and repeat."
msgid "E_D_GLUTE_KICKBACK_BAND_EXEC_FOUR"
msgstr "Switch to the other side and repeat the same repetitions."
msgid "E_D_GLUTE_KICKBACK_BAND_TIP_ONE"
msgstr "Keep your pelvis staying pointed toward the ground,"
msgid "E_D_GLUTE_KICKBACK_BAND_TIP_TWO"
msgstr "Keep your torso steady with a straight back and braced core during the exercise."
msgid "E_D_GOOD_MORNING_BAND_TITLE"
msgstr "Good Morning: Band"
msgid "E_D_GOOD_MORNING_BAND_FOCUS"
msgstr "Lower back, Glutes, Hamstrings"
msgid "E_D_GOOD_MORNING_BAND_EQUIP"
msgstr "Handle Band"
msgid "E_D_GOOD_MORNING_BAND_PREP_ONE"
msgstr "Stand up straight and place the band under your midfoot."
msgid "E_D_GOOD_MORNING_BAND_PREP_TWO"
msgstr "Bend at the elbows and hold the handles across the shoulders with palms facing each other."
msgid "E_D_GOOD_MORNING_BAND_EXEC_ONE"
msgstr "Hinge forwards from your hips and slightly bend in your knees until your upper body is parallel to the floor, Feel the stretch in your lower back and hamstrings."
msgid "E_D_GOOD_MORNING_BAND_EXEC_TWO"
msgstr "Return to the starting position and repeat."
msgid "E_D_GOOD_MORNING_BAND_TIP_ONE"
msgstr "Keep your head, spine and neck in line throughout the movement."
msgid "E_D_GOOD_MORNING_BAND_TIP_TWO"
msgstr "Keep a braced core during the exercise."
msgid "E_D_GOOD_MORNING_BARBELL_TITLE"
msgstr "Good Morning: Barbell"
msgid "E_D_GOOD_MORNING_BARBELL_FOCUS"
msgstr "Glutes, Lower back, Hamstrings"
msgid "E_D_GOOD_MORNING_BARBELL_EQUIP"
msgstr "Barbell"
msgid "E_D_GOOD_MORNING_BARBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_GOOD_MORNING_BARBELL_PREP_TWO"
msgstr "Place the barbell across your shoulders and hold it with palms facing forward."
msgid "E_D_GOOD_MORNING_BARBELL_EXEC_ONE"
msgstr "Hinge forwards from your hips and slightly bend in your knees until your upper body is parallel to the floor, Feel the stretch in your lower back and hamstrings."
msgid "E_D_GOOD_MORNING_BARBELL_EXEC_TWO"
msgstr "Return to the starting position and repeat."
msgid "E_D_GOOD_MORNING_BARBELL_TIP_ONE"
msgstr "Keep your head, spine and neck in line throughout the movement."
msgid "E_D_GOOD_MORNING_BARBELL_TIP_TWO"
msgstr "Keep a braced core during the exercise."
msgid "E_D_GOOD_MORNING_SMITH_MACHINE_TITLE"
msgstr "Good Morning: Smith Machine"
msgid "E_D_GOOD_MORNING_SMITH_MACHINE_FOCUS"
msgstr "Glutes, Lower back, Hamstrings"
msgid "E_D_GOOD_MORNING_SMITH_MACHINE_EQUIP"
msgstr "Smith Machine"
msgid "E_D_GOOD_MORNING_SMITH_MACHINE_PREP_ONE"
msgstr "Stand in the machine with feet shoulder width apart"
msgid "E_D_GOOD_MORNING_SMITH_MACHINE_PREP_TWO"
msgstr "Place the bar across your shoulders and hold it with palms facing forward."
msgid "E_D_GOOD_MORNING_SMITH_MACHINE_EXEC_ONE"
msgstr "Hinge forwards from your hips and slightly bend in your knees until your upper body is parallel to the floor, Feel the stretch in your lower back and hamstrings."
msgid "E_D_GOOD_MORNING_SMITH_MACHINE_EXEC_TWO"
msgstr "Return to the starting position and repeat."
msgid "E_D_GOOD_MORNING_SMITH_MACHINE_TIP_ONE"
msgstr "Keep your head, spine and neck in line throughout the movement."
msgid "E_D_GOOD_MORNING_SMITH_MACHINE_TIP_TWO"
msgstr "Keep a braced core during the exercise."
msgid "E_D_GORILLA_CHIN_CRUNCH_TITLE"
msgstr "Gorilla Chin Crunch"
msgid "E_D_GORILLA_CHIN_CRUNCH_FOCUS"
msgstr "Abs, Quadriceps"
msgid "E_D_GORILLA_CHIN_CRUNCH_EQUIP"
msgstr "Bodyweight"
msgid "E_D_GORILLA_CHIN_CRUNCH_PREP_ONE"
msgstr "Hang on the bar with your hands shoulder width apart and your palms facing backward."
msgid "E_D_GORILLA_CHIN_CRUNCH_PREP_TWO"
msgstr "Keep your body straight and bend the knees at 90 degrees."
msgid "E_D_GORILLA_CHIN_CRUNCH_EXEC_ONE"
msgstr "Exhale and pull yourself upwards until your chin is at the level of the bar while bringing your knees up to your chest."
msgid "E_D_GORILLA_CHIN_CRUNCH_EXEC_TWO"
msgstr "Inhale and lower down to the starting position slowly and repeat."
msgid "E_D_GORILLA_CHIN_CRUNCH_TIP_ONE"
msgstr "Do not swing your legs up to create any momentum."
msgid "E_D_GORILLA_CHIN_CRUNCH_TIP_TWO"
msgstr "Cross your ankles to stabilize the position."
msgid "E_D_HACK_SQUAT_BARBELL_TITLE"
msgstr "Hack Squat: Barbell"
msgid "E_D_HACK_SQUAT_BARBELL_FOCUS"
msgstr "Quadriceps, Glutes"
msgid "E_D_HACK_SQUAT_BARBELL_EQUIP"
msgstr "Barbell"
msgid "E_D_HACK_SQUAT_BARBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_HACK_SQUAT_BARBELL_PREP_TWO"
msgstr "Hold the barbell positioned behind your hips with hands shoulder width apart and palms facing backward."
msgid "E_D_HACK_SQUAT_BARBELL_EXEC_ONE"
msgstr "Inhale and squat down until your thighs are parallel to the floor."
msgid "E_D_HACK_SQUAT_BARBELL_EXEC_TWO"
msgstr "Hold for a few seconds to feel the contraction."
msgid "E_D_HACK_SQUAT_BARBELL_EXEC_THREE"
msgstr "Exhale, drive through the heels and midfoot to the starting position slowly and repeat."
msgid "E_D_HACK_SQUAT_BARBELL_TIP_ONE"
msgstr "Keep the tiptoes in the same direction as your knees."
msgid "E_D_HACK_SQUAT_BARBELL_TIP_TWO"
msgstr "Do not let your knees go over your toes."
msgid "E_D_HACK_SQUAT_BARBELL_TIP_THREE"
msgstr "Do not let the bar touch the floor during the exercise."
msgid "E_D_HACK_SQUAT_BARBELL_TIP_FOUR"
msgstr "Keep your glutes, quadriceps and hamstrings engaged"
msgid "E_D_HACK_SQUAT_MACHINE_TITLE"
msgstr "Hack Squat: Machine"
msgid "E_D_HACK_SQUAT_MACHINE_FOCUS"
msgstr "Glutes, Quadriceps"
msgid "E_D_HACK_SQUAT_MACHINE_EQUIP"
msgstr "Hack Squat Machine"
msgid "E_D_HACK_SQUAT_MACHINE_PREP_ONE"
msgstr "Lie on the machine with shoulders against the pads and feet together."
msgid "E_D_HACK_SQUAT_MACHINE_PREP_TWO"
msgstr "Hold the handles in a neutral grip"
msgid "E_D_HACK_SQUAT_MACHINE_EXEC_ONE"
msgstr "Inhale and squat down until your knees bent at 90 degrees."
msgid "E_D_HACK_SQUAT_MACHINE_EXEC_TWO"
msgstr "Hold for a few seconds to feel the contraction."
msgid "E_D_HACK_SQUAT_MACHINE_EXEC_THREE"
msgstr "Exhale, drive through the heels and midfoot to the starting position slowly and repeat."
msgid "E_D_HACK_SQUAT_MACHINE_TIP_ONE"
msgstr "Keep the tiptoes in the same direction as your knees."
msgid "E_D_HACK_SQUAT_MACHINE_TIP_TWO"
msgstr "Do not let your knees go over your toes."
msgid "E_D_HACK_SQUAT_MACHINE_TIP_THREE"
msgstr "Do not let the bar touch the floor during the exercise."
msgid "E_D_HACK_SQUAT_MACHINE_TIP_FOUR"
msgstr "Keep your glutes, quadriceps and hamstrings engaged."
msgid "E_D_HAMMER_CURL_BAND_TITLE"
msgstr "Hammer Curl: Band"
msgid "E_D_HAMMER_CURL_BAND_FOCUS"
msgstr "Biceps, Forearms"
msgid "E_D_HAMMER_CURL_BAND_EQUIP"
msgstr "Handle Band"
msgid "E_D_HAMMER_CURL_BAND_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_HAMMER_CURL_BAND_PREP_TWO"
msgstr "Hold a handle in each hand in front of your thighs with palms facing each other. Extend arms tucked to your sides."
msgid "E_D_HAMMER_CURL_BAND_EXEC_ONE"
msgstr "Squeeze your biceps to bend your elbows and curl the band up to the height of your chest."
msgid "E_D_HAMMER_CURL_BAND_EXEC_TWO"
msgstr "Return to the starting position slowly and repeat."
msgid "E_D_HAMMER_CURL_BAND_TIP_ONE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_HAMMER_CURL_BAND_TIP_TWO"
msgstr "Keep your elbows still to your sides throughout the movement."
msgid "E_D_HAMMER_CURL_CABLE_TITLE"
msgstr "Hammer Curl: Cable"
msgid "E_D_HAMMER_CURL_CABLE_FOCUS"
msgstr "Biceps, Forearms"
msgid "E_D_HAMMER_CURL_CABLE_EQUIP"
msgstr "Cable Machine"
msgid "E_D_HAMMER_CURL_CABLE_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_HAMMER_CURL_CABLE_PREP_TWO"
msgstr "Hold the rope attachment in front of your thighs with palms facing each other. Extend arms tucked to your sides."
msgid "E_D_HAMMER_CURL_CABLE_EXEC_ONE"
msgstr "Squeeze your biceps to bend your elbows and curl the rope up to the height of your chest."
msgid "E_D_HAMMER_CURL_CABLE_EXEC_TWO"
msgstr "Return to the starting position slowly and repeat."
msgid "E_D_HAMMER_CURL_CABLE_TIP_ONE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_HAMMER_CURL_CABLE_TIP_TWO"
msgstr "Keep your elbows still to your sides throughout the movement."
msgid "E_D_HAMMER_CURL_DUMBBELL_TITLE"
msgstr "Hammer Curl: Dumbbell"
msgid "E_D_HAMMER_CURL_DUMBBELL_FOCUS"
msgstr "Biceps, Forearms"
msgid "E_D_HAMMER_CURL_DUMBBELL_EQUIP"
msgstr "Dumbbell"
msgid "E_D_HAMMER_CURL_DUMBBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_HAMMER_CURL_DUMBBELL_PREP_TWO"
msgstr "Hold a dumbbell in each hand in front of your thighs with. palms facing each other. Extend arms tucked to you sides."
msgid "E_D_HAMMER_CURL_DUMBBELL_EXEC_ONE"
msgstr "Squeeze your biceps to bend your elbows and curl the dumbbells up to your chest."
msgid "E_D_HAMMER_CURL_DUMBBELL_EXEC_TWO"
msgstr "Return to the starting position slowly and repeat."
msgid "E_D_HAMMER_CURL_DUMBBELL_TIP_ONE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_HAMMER_CURL_DUMBBELL_TIP_TWO"
msgstr "Keep your elbows still to your sides throughout the movement."
msgid "E_D_HANG_CLEAN_BARBELL_TITLE"
msgstr "Hang Clean: Barbell"
msgid "E_D_HANG_CLEAN_BARBELL_FOCUS"
msgstr "Full Body"
msgid "E_D_HANG_CLEAN_BARBELL_EQUIP"
msgstr "Barbell"
msgid "E_D_HANG_CLEAN_BARBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_HANG_CLEAN_BARBELL_PREP_TWO"
msgstr "Hold the bar with ands slightly wider than shoulder width apart in an overhand grip and arms extended."
msgid "E_D_HANG_CLEAN_BARBELL_EXEC_ONE"
msgstr "Drive your hips up and straighten your legs to pull the barbell up."
msgid "E_D_HANG_CLEAN_BARBELL_EXEC_TWO"
msgstr "Once the barbell passes your mid-thigh, jump slightly and drop your body under the bar with your elbows facing forward and palms facing up."
msgid "E_D_HANG_CLEAN_BARBELL_EXEC_THREE"
msgstr "As you receive the barbell, lower your hips to a front squat position."
msgid "E_D_HANG_CLEAN_BARBELL_EXEC_FOUR"
msgstr "Brace you core and drive through your feet to stand up straight."
msgid "E_D_HANG_CLEAN_BARBELL_EXEC_FIVE"
msgstr "Lower the barbell to the starting position slowly and repeat."
msgid "E_D_HANG_CLEAN_BARBELL_TIP_ONE"
msgstr "Do not let your knees go over your toes when you squat."
msgid "E_D_HANG_CLEAN_BARBELL_TIP_TWO"
msgstr "To add difficulty, add weights gradually."
msgid "E_D_HANG_CLEAN_BARBELL_TIP_THREE"
msgstr "Keep your knees and elbows slightly bent during the exercise."
msgid "E_D_HANG_SNATCH_BARBELL_TITLE"
msgstr "Hang Snatch: Barbell"
msgid "E_D_HANG_SNATCH_BARBELL_FOCUS"
msgstr "Full Body"
msgid "E_D_HANG_SNATCH_BARBELL_EQUIP"
msgstr "Barbell"
msgid "E_D_HANG_SNATCH_BARBELL_PREP_ONE"
msgstr "Stand with feet shoulder width apart."
msgid "E_D_HANG_SNATCH_BARBELL_PREP_TWO"
msgstr "Hold the barbell with hands slightly wider than shoulder width apart in an overhand grip and arms extended."
msgid "E_D_HANG_SNATCH_BARBELL_EXEC_ONE"
msgstr "Shrug your shoulders to lift the barbell with your arms straight."
msgid "E_D_HANG_SNATCH_BARBELL_EXEC_TWO"
msgstr "Once the bar passes your mid-thigh, jump slightly and drop your body under the bar while lifting the barbell overhead with palms facing forward."
msgid "E_D_HANG_SNATCH_BARBELL_EXEC_THREE"
msgstr "As you lift the barbell, lower your hips to front squat position."
msgid "E_D_HANG_SNATCH_BARBELL_EXEC_FOUR"
msgstr "Brace you core and drive through your feet to stand up straight."
msgid "E_D_HANG_SNATCH_BARBELL_EXEC_FIVE"
msgstr "Lower the barbell to the starting position slowly and repeat."
msgid "E_D_HANG_SNATCH_BARBELL_TIP_ONE"
msgstr "Do not let your knees go over your toes when you squat."
msgid "E_D_HANG_SNATCH_BARBELL_TIP_TWO"
msgstr "To add difficulty, add weights gradually."
msgid "E_D_HANG_SNATCH_BARBELL_TIP_THREE"
msgstr "Keep your knees and elbows slightly bent during the exercise."
msgid "E_D_HANG_SNATCH_BARBELL_TIP_FOUR"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_HANGING_KNEE_RAISE_TITLE"
msgstr "Hanging Knee Raise"
msgid "E_D_HANGING_KNEE_RAISE_FOCUS"
msgstr "Abs, Quadriceps"
msgid "E_D_HANGING_KNEE_RAISE_EQUIP"
msgstr "Bodyweight"
msgid "E_D_HANGING_KNEE_RAISE_PREP_ONE"
msgstr "Hang on the bar with hands shoulder width apart and palms facing forward."
msgid "E_D_HANGING_KNEE_RAISE_PREP_TWO"
msgstr "Keep your arms extended and legs straight down."
msgid "E_D_HANGING_KNEE_RAISE_EXEC_ONE"
msgstr "Exhale and squeeze your abs to lift your knees to your chest."
msgid "E_D_HANGING_KNEE_RAISE_EXEC_TWO"
msgstr "Pause for a few seconds, inhale and return to the starting position slowly and repeat."
msgid "E_D_HANGING_KNEE_RAISE_TIP_ONE"
msgstr "Do not shrug your shoulders when pulling."
msgid "E_D_HANGING_KNEE_RAISE_TIP_TWO"
msgstr "To add difficulty, use ankle weights."
msgid "E_D_HANGING_KNEE_RAISE_TIP_THREE"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_HANGING_LEG_RAISE_TITLE"
msgstr "Hanging Leg Raise"
msgid "E_D_HANGING_LEG_RAISE_FOCUS"
msgstr "Abs, Quadriceps"
msgid "E_D_HANGING_LEG_RAISE_EQUIP"
msgstr "Bodyweight"
msgid "E_D_HANGING_LEG_RAISE_PREP_ONE"
msgstr "Hang on the bar with hands shoulder width apart and palms facing forward."
msgid "E_D_HANGING_LEG_RAISE_PREP_TWO"
msgstr "Keep your arms extended and legs straight down."
msgid "E_D_HANGING_LEG_RAISE_EXEC_ONE"
msgstr "Exhale and squeeze your abs to raise your legs until they are parallel to the floor."
msgid "E_D_HANGING_LEG_RAISE_EXEC_TWO"
msgstr "Pause for a few seconds, inhale and return to the starting position slowly and repeat."
msgid "E_D_HANGING_LEG_RAISE_TIP_ONE"
msgstr "Do not shrug your shoulders when pulling"
msgid "E_D_HANGING_LEG_RAISE_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_HANGING_LEG_RAISE_TIP_THREE"
msgstr "Keep your legs straight throughout the movement."
msgid "E_D_HANGING_LEG_RAISE_TWIST_TITLE"
msgstr "Hanging Leg Raise Twist"
msgid "E_D_HANGING_LEG_RAISE_TWIST_FOCUS"
msgstr "Abs, Quadriceps"
msgid "E_D_HANGING_LEG_RAISE_TWIST_EQUIP"
msgstr "Bodyweight"
msgid "E_D_HANGING_LEG_RAISE_TWIST_PREP_ONE"
msgstr "Hang on the bar with hands shoulder width apart and palms facing forward."
msgid "E_D_HANGING_LEG_RAISE_TWIST_PREP_TWO"
msgstr "Keep your arms extended and legs straight down."
msgid "E_D_HANGING_LEG_RAISE_TWIST_EXEC_ONE"
msgstr "Exhale and squeeze your abs, especially obliques to lift your legs together towards one side."
msgid "E_D_HANGING_LEG_RAISE_TWIST_EXEC_TWO"
msgstr "Pause for a few seconds, inhale and return to the starting position slowly and repeat."
msgid "E_D_HANGING_LEG_RAISE_TWIST_EXEC_THREE"
msgstr "Complete alternating between left and right."
msgid "E_D_HANGING_LEG_RAISE_TWIST_TIP_ONE"
msgstr "Do not rug your shoulders when pulling"
msgid "E_D_HANGING_LEG_RAISE_TWIST_TIP_TWO"
msgstr "Keep a straight back and braced core during the exercise."
msgid "E_D_HANGING_LEG_RAISE_TWIST_TIP_THREE"
msgstr "Keep your legs straight throughout the movement."
msgid "E_D_HEEL_TOUCH_TITLE"
msgstr "Heel Touch"
msgid "E_D_HEEL_TOUCH_FOCUS"
msgstr "Abs"
msgid "E_D_HEEL_TOUCH_EQUIP"
msgstr "Bodyweight"
msgid "E_D_HEEL_TOUCH_PREP_ONE"
msgstr "Lie with knees bent, feet together on the floor and arms extended at your sides."
msgid "E_D_HEEL_TOUCH_PREP_TWO"
msgstr "Squeeze your abs to crunch your torso."
msgid "E_D_HEEL_TOUCH_EXEC_ONE"
msgstr "Twist your torso to one side until you touch the heel with the hand on the same side."
msgid "E_D_HEEL_TOUCH_EXEC_TWO"
msgstr "Return to the starting position slowly and complete alternating between left and right"
msgid "E_D_HEEL_TOUCH_TIP_ONE"
msgstr "Move your entire trunk instead of only straining your neck."
msgid "E_D_HEEL_TOUCH_TIP_TWO"
msgstr "Keep a braced core during the exercise."
