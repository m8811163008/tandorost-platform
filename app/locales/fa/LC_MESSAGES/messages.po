msgid "username_in_use"
msgstr "نام کاربری '{user_name}' قبلاً استفاده شده است"

msgid "welcome_message"
msgstr "خوش آمدید، آدرس API: {api_uri}."

msgid "invalid_credentials"
msgstr "اعتبارسنجی اطلاعات ورود انجام نشد."

msgid "incorrect_credentials"
msgstr "نام کاربری یا رمز عبور اشتباه است"

msgid "remote_service_unavailable"
msgstr "سرویس راه دور در دسترس نیست. دوباره تلاش کنید"

msgid "user_registered"
msgstr "خوش آمدید، ثبت‌نام شما با موفقیت انجام شد"

msgid "rate_limit_reach"
msgstr "محدودیت تعداد درخواست‌ها رسید. {second} ثانیه بعد دوباره تلاش کنید"

msgid "username_not_registered_yet"
msgstr "لطفاً ابتدا نام کاربری را برای دریافت کد تأیید ثبت کنید"

msgid "invalid_verification_code"
msgstr "کد تأیید نامعتبر است. دوباره بررسی کنید."

msgid "username_is_inactive"
msgstr "نام کاربری غیرفعال است. با پشتیبانی تماس بگیرید."

msgid "invalid_password"
msgstr "رمز عبور نامعتبر است. رمز را بررسی یا بازیابی کنید"

msgid "password_restored"
msgstr "رمز عبور با موفقیت بازیابی شد. اکنون می‌توانید وارد شوید."

msgid "invalid_token"
msgstr "توکن JSON Web نامعتبر است."

msgid "user_not_found"
msgstr "شناسه کاربر نامعتبر است."

msgid "object_not_found"
msgstr "شیء پیدا نشد."

msgid "verification_code_send"
msgstr "کد تأیید با موفقیت ارسال شد."

msgid "invalid_upload_file_request"
msgstr "درخواست بارگذاری فایل نامعتبر است."

msgid "file_upload_failed"
msgstr "بارگذاری فایل انجام نشد. لطفاً دوباره تلاش کنید."

msgid "poor_quality"
msgstr "تصویر تار، پیکسلی یا با کیفیت پایین است."

msgid "inappropriate_content"
msgstr "تصویر شامل برهنگی، محتوای توهین‌آمیز یا نامناسب است."

msgid "irrelevant_content"
msgstr "تصویر مرتبط با تناسب اندام یا هدف مورد نظر نیست."

msgid "cropped_or_obstructed"
msgstr "بخش‌های کلیدی تصویر بریده یا پوشانده شده‌اند."

msgid "poor_lighting"
msgstr "تصویر بیش از حد تاریک، روشن یا دارای سایه زیاد است."

msgid "unsafe_activity"
msgstr "تصویر فعالیت‌های ناایمن یا بالقوه خطرناک را نشان می‌دهد."

msgid "lack_of_consent"
msgstr "تصویر احتمالاً شامل افرادی است که رضایت برای نمایش ندارند."

msgid "violates_guidelines"
msgstr "تصویر با قوانین جامعه یا شرایط سرویس مغایرت دارد."

msgid "file_format_unsupported"
msgstr "فرمت فایل بارگذاری شده پشتیبانی نمی‌شود."

msgid "file_size_too_large"
msgstr "حجم فایل بارگذاری شده بیش از حد مجاز است."

msgid "watermark_or_logo"
msgstr "تصویر دارای واترمارک یا لوگوی قابل مشاهده است (در صورت ممنوعیت)."

msgid "misleading_or_false"
msgstr "تصویر گمراه‌کننده یا نتیجه/محصول تناسب اندام را به‌درستی نشان نمی‌دهد."

msgid "other_rejection_reason"
msgstr "دلیل دیگری که در بالا ذکر نشده است."

msgid "file_limit_exceeded"
msgstr "حجم فایل از حد مجاز {file_size_limit} مگابایت بیشتر است."

msgid "invalid_argument"
msgstr "بدنه درخواست ناقص یا اشتباه است. یک فیلد ضروری جا افتاده یا اشتباه تایپی وجود دارد. برای فرمت درخواست، مثال‌ها و نسخه‌های پشتیبانی شده به مستندات API مراجعه کنید."

msgid "failed_precondition"
msgstr "نسخه رایگان Gemini API در کشور شما در دسترس نیست. لطفاً صورت‌حساب را در پروژه خود در Google AI Studio فعال کنید. برای استفاده از Gemini API باید یک طرح پولی راه‌اندازی کنید."

msgid "permission_denied"
msgstr "کلید API شما مجوزهای لازم را ندارد. کلید اشتباه است یا سعی دارید از مدل تنظیم‌شده بدون احراز هویت مناسب استفاده کنید. مطمئن شوید کلید API شما تنظیم و دارای دسترسی صحیح است."

msgid "not_found"
msgstr "منبع درخواستی پیدا نشد. فایل تصویر، صوت یا ویدیوی ارجاع داده شده در درخواست شما یافت نشد. بررسی کنید همه پارامترها برای نسخه API شما معتبر باشند."

msgid "resource_exhausted"
msgstr "محدودیت نرخ درخواست‌ها را رد کرده‌اید. تعداد درخواست‌های شما در دقیقه با نسخه رایگان Gemini API بیش از حد مجاز است. مطمئن شوید در محدوده مجاز مدل هستید. در صورت نیاز درخواست افزایش سهمیه دهید."

msgid "internal_error"
msgstr "خطای غیرمنتظره‌ای در سمت گوگل رخ داد. زمینه ورودی شما خیلی طولانی است. زمینه ورودی را کاهش دهید یا موقتاً به مدل دیگری (مثلاً از Gemini 1.5 Pro به Gemini 1.5 Flash) تغییر دهید و دوباره امتحان کنید. یا کمی صبر کنید و دوباره تلاش کنید."

msgid "service_unavailable"
msgstr "سرویس ممکن است موقتاً بیش از حد بارگذاری شده یا قطع باشد. سرویس موقتاً ظرفیت کافی ندارد. موقتاً به مدل دیگری (مثلاً از Gemini 1.5 Pro به Gemini 1.5 Flash) تغییر دهید و دوباره امتحان کنید. یا کمی صبر کنید و دوباره تلاش کنید."

msgid "deadline_exceeded"
msgstr "سرویس قادر به پردازش درخواست در زمان مقرر نیست. درخواست یا زمینه شما برای پردازش در زمان کافی بزرگ است. برای جلوگیری از این خطا، مقدار 'timeout' را در درخواست خود افزایش دهید."

msgid "verification_email_subject"
msgstr "کد تأیید"

msgid "verification_email_body"
msgstr "خوش آمدید، کد تأیید شما: {code}."


msgid "E_D_AB_WHEEL_TITLE"
msgstr "Ab Wheel"
msgid "E_D_AB_WHEEL_PREP_ONE"
msgstr "غلتک چرخ را درست زیر شانه‌های خود روی زمین قرار دهید."
msgid "E_D_AB_WHEEL_PREP_TWO"
msgstr "روی زمین زانو بزنید و دسته‌های غلتک چرخ را با بازوهای کشیده نگه دارید."
msgid "E_D_AB_WHEEL_EXEC_ONE"
msgstr "دم بگیرید و به آرامی غلتک را به جلو هل دهید تا تنه شما موازی زمین شود."
msgid "E_D_AB_WHEEL_EXEC_TWO"
msgstr "وقتی بدن کاملاً کشیده شد، چند ثانیه مکث کنید."
msgid "E_D_AB_WHEEL_EXEC_THREE"
msgstr "بازدم کنید و غلتک را به سمت خود بکشید تا به حالت شروع بازگردید و تکرار کنید."
msgid "E_D_AB_WHEEL_TIP_ONE"
msgstr "برای افزودن دشواری، فقط پاهای خود را روی زمین قرار دهید."
msgid "E_D_AB_WHEEL_TIP_TWO"
msgstr "در طول تمرین، کمر خود را صاف و عضلات مرکزی (Core) را منقبض نگه دارید."
msgid "E_D_ALTERNATING_PUNCH_TITLE"
msgstr "Alternating Punch"
msgid "E_D_ALTERNATING_PUNCH_PREP_ONE"
msgstr "بایستید و پاهای خود را به عرض شانه باز کنید."
msgid "E_D_ALTERNATING_PUNCH_PREP_TWO"
msgstr "زانوهای خود را کمی خم نگه دارید."
msgid "E_D_ALTERNATING_PUNCH_EXEC_ONE"
msgstr "یک مشت را از بدن خود دور کرده و بچرخانید، سپس سریعاً آن را به حالت شروع برگردانید."
msgid "E_D_ALTERNATING_PUNCH_EXEC_TWO"
msgstr "بازوی مخالف را با یک مشت سریع به جلو پرتاب کنید و تکرار کنید."
msgid "E_D_ALTERNATING_PUNCH_TIP_ONE"
msgstr "به منظور افزایش تنش در عضلات شکم، این حرکت را با سرعت بیشتری انجام دهید."
msgid "E_D_ALTERNATING_PUNCH_TIP_TWO"
msgstr "کمر خود را صاف نگه دارید و عضلات مرکزی (Core) را در طول تمرین منقبض کنید."
msgid "E_D_BANDED_KNEE_EXTENSION_TITLE"
msgstr "Banded Knee Extension"
msgid "E_D_BANDED_KNEE_EXTENSION_PREP_ONE"
msgstr "روی لبه یک صندلی محکم بنشینید."
msgid "E_D_BANDED_KNEE_EXTENSION_PREP_TWO"
msgstr "یک بند مقاومتی (Resistance Band) را دور مچ پا یا کف پای خود قرار دهید و انتهای آن را با دست در کنار صندلی نگه دارید."
msgid "E_D_BANDED_KNEE_EXTENSION_EXEC_ONE"
msgstr "پای خود را در مقابل نیروی باند کششی تا جایی که کاملاً صاف شود، بالا بیاورید."
msgid "E_D_BANDED_KNEE_EXTENSION_EXEC_TWO"
msgstr "چند ثانیه نگه دارید، سپس به آرامی به موقعیت شروع بازگردید."
msgid "E_D_BANDED_KNEE_EXTENSION_TIP_ONE"
msgstr "از خم کردن کمر خودداری کنید و در طول تمرین عضلات مرکزی (Core) را منقبض نگه دارید."
msgid "E_D_BANDED_KNEE_EXTENSION_TIP_TWO"
msgstr "بیشتر از زانوهای خود خم نشوید."
msgid "E_D_BARBELL_FLOOR_PRESS_TITLE"
msgstr "Barbell Floor Press"
msgid "E_D_BARBELL_FLOOR_PRESS_PREP_ONE"
msgstr "به پشت روی زمین دراز بکشید و زانوهای خود را خم کنید، کف پاهایتان روی زمین قرار بگیرد."
msgid "E_D_BARBELL_FLOOR_PRESS_PREP_TWO"
msgstr "میله هالتر (Barbell) را با دستانی کمی بازتر از عرض شانه و دستگیره خنثی (Neutral Grip) بگیرید."
msgid "E_D_BARBELL_FLOOR_PRESS_EXEC_ONE"
msgstr "میله را به سمت سینه خود پایین بیاورید تا بازوها با زمین موازی شوند."
msgid "E_D_BARBELL_FLOOR_PRESS_EXEC_TWO"
msgstr "مکث کنید و سپس با یک حرکت کنترل شده میله را به سمت بالا فشار دهید."
msgid "E_D_BARBELL_FLOOR_PRESS_TIP_ONE"
msgstr "مطمئن شوید که مچ دست و بازوهای شما عمود بر زمین هستند."
msgid "E_D_BARBELL_FLOOR_PRESS_TIP_TWO"
msgstr "آرنج‌ها را در بالای حرکت قفل نکنید."
msgid "E_D_BARBELL_LUNGES_TITLE"
msgstr "Barbell Lunges"
msgid "E_D_BARBELL_LUNGES_PREP_ONE"
msgstr "هالتر را روی قسمت بالایی کمر (کول) خود قرار دهید."
msgid "E_D_BARBELL_LUNGES_PREP_TWO"
msgstr "بایستید و پاهایتان را به عرض باسن باز کنید."
msgid "E_D_BARBELL_LUNGES_EXEC_ONE"
msgstr "یک گام بزرگ به جلو بردارید و بدن خود را به سمت پایین بیاورید تا زانوی جلویی شما 90 درجه خم شود."
msgid "E_D_BARBELL_LUNGES_EXEC_TWO"
msgstr "وقتی زانوی عقبی شما نزدیک زمین شد، با پای جلویی خود به بالا فشار دهید و به موقعیت شروع بازگردید."
msgid "E_D_BARBELL_LUNGES_TIP_ONE"
msgstr "کمر خود را در طول تمرین صاف نگه دارید."
msgid "E_D_BARBELL_LUNGES_TIP_TWO"
msgstr "مطمئن شوید که زانوی جلویی از نوک انگشتان پا جلوتر نرود."
msgid "E_D_BARBELL_SQUAT_TITLE"
msgstr "Barbell Squat"
msgid "E_D_BARBELL_SQUAT_PREP_ONE"
msgstr "هالتر را روی قسمت بالایی کمر (کول) خود قرار دهید."
msgid "E_D_BARBELL_SQUAT_PREP_TWO"
msgstr "با پاهای خود کمی بازتر از عرض شانه و نوک انگشتان پا کمی به بیرون بایستید."
msgid "E_D_BARBELL_SQUAT_EXEC_ONE"
msgstr "همانطور که لگن خود را به عقب هل می‌دهید، به آرامی بدن خود را پایین بیاورید انگار که می‌خواهید روی یک صندلی بنشینید."
msgid "E_D_BARBELL_SQUAT_EXEC_TWO"
msgstr "وقتی ران‌های شما موازی با زمین شدند، به بالا فشار دهید و به موقعیت شروع بازگردید."
msgid "E_D_BARBELL_SQUAT_TIP_ONE"
msgstr "کمر خود را صاف و عضلات مرکزی (Core) را منقبض کنید."
msgid "E_D_BARBELL_SQUAT_TIP_TWO"
msgstr "مطمئن شوید که زانوهای شما با انگشتان پا در یک راستا حرکت می‌کنند."
msgid "E_D_BATTLE_ROPE_ALTERNATING_WAVES_TITLE"
msgstr "Battle Rope Alternating Waves"
msgid "E_D_BATTLE_ROPE_ALTERNATING_WAVES_PREP_ONE"
msgstr "طناب بتل روپ (Battle Rope) را بگیرید و با پاهایتان به عرض شانه و زانوهای کمی خمیده بایستید."
msgid "E_D_BATTLE_ROPE_ALTERNATING_WAVES_PREP_TWO"
msgstr "کمی به جلو خم شوید، کمر خود را صاف نگه دارید و عضلات مرکزی (Core) را منقبض کنید."
msgid "E_D_BATTLE_ROPE_ALTERNATING_WAVES_EXEC_ONE"
msgstr "بازوهای خود را به طور متناوب بالا و پایین ببرید تا امواج متناوب در طناب ایجاد شود."
msgid "E_D_BATTLE_ROPE_ALTERNATING_WAVES_EXEC_TWO"
msgstr "حرکت را تکرار کنید و موج‌ها را حفظ کنید."
msgid "E_D_BATTLE_ROPE_ALTERNATING_WAVES_TIP_ONE"
msgstr "حرکت را از شانه‌های خود شروع کنید."
msgid "E_D_BATTLE_ROPE_ALTERNATING_WAVES_TIP_TWO"
msgstr "سرعت امواج را با حرکت سریع‌تر بازوهای خود افزایش دهید."
msgid "E_D_BATTLE_ROPE_SLAM_TITLE"
msgstr "Battle Rope Slam"
msgid "E_D_BATTLE_ROPE_SLAM_PREP_ONE"
msgstr "طناب بتل روپ را بگیرید و با پاهایتان به عرض شانه و زانوهای کمی خمیده بایستید."
msgid "E_D_BATTLE_ROPE_SLAM_PREP_TWO"
msgstr "کمی به جلو خم شوید، کمر خود را صاف نگه دارید و عضلات مرکزی (Core) را منقبض کنید."
msgid "E_D_BATTLE_ROPE_SLAM_EXEC_ONE"
msgstr "با هر دو دست طناب را بالای سر خود ببرید."
msgid "E_D_BATTLE_ROPE_SLAM_EXEC_TWO"
msgstr "طناب را با تمام قدرت به زمین بکوبید و زانوهای خود را کمی خم کنید."
msgid "E_D_BATTLE_ROPE_SLAM_TIP_ONE"
msgstr "انفجاری عمل کنید و تمام نیروی خود را در کوبیدن طناب به کار ببرید."
msgid "E_D_BATTLE_ROPE_SLAM_TIP_TWO"
msgstr "هنگام پایین آوردن طناب، به سمت پایین حرکت کنید و از قدرت باسن برای بلند کردن آن استفاده کنید."
msgid "E_D_BENCH_PRESS_BARBELL_TITLE"
msgstr "Bench Press: Barbell"
msgid "E_D_BENCH_PRESS_BARBELL_PREP_ONE"
msgstr "به پشت روی نیمکت دراز بکشید و پاهای خود را روی زمین قرار دهید."
msgid "E_D_BENCH_PRESS_BARBELL_PREP_TWO"
msgstr "میله را در ارتفاع قفسه سینه با دستانی بازتر از عرض شانه، بازوهای خم شده و کف دست‌ها رو به جلو نگه دارید."
msgid "E_D_BENCH_PRESS_BARBELL_EXEC_ONE"
msgstr "بازدم کنید و سینه خود را فشار دهید تا میله را به سمت بالا فشار دهید و بازوهای خود را صاف کنید."
msgid "E_D_BENCH_PRESS_BARBELL_EXEC_TWO"
msgstr "دم بگیرید و میله را به آرامی به موقعیت شروع پایین بیاورید و تکرار کنید."
msgid "E_D_BENCH_PRESS_BARBELL_TIP_ONE"
msgstr "آرنج‌های خود را در بالا قفل نکنید."
msgid "E_D_BENCH_PRESS_BARBELL_TIP_TWO"
msgstr "در طول تمرین، کمر خود را صاف و عضلات مرکزی (Core) را منقبض نگه دارید."
msgid "E_D_BENCH_PRESS_CABLE_TITLE"
msgstr "Bench Press: Cable"
msgid "E_D_BENCH_PRESS_CABLE_FOCUS_AREA"
msgstr "سینه، شانه، پشت بازو (Triceps)"
msgid "E_D_BENCH_PRESS_CABLE_EQUIPMENT"
msgstr "دستگاه سیم‌کش، نیمکت صاف"
msgid "E_D_BENCH_PRESS_CABLE_PREP_ONE"
msgstr "روی نیمکت دراز بکشید و پاهایتان را روی زمین قرار دهید."
msgid "E_D_BENCH_PRESS_CABLE_PREP_TWO"
msgstr "یک دستگیره را در هر دست با دستگیره خنثی (Neutral Grip) به عرض شانه و آرنج‌های خم شده در هر طرف سینه خود بگیرید."
msgid "E_D_BENCH_PRESS_CABLE_EXEC_ONE"
msgstr "نفس خود را بیرون داده و با فشار دادن دستگیره‌ها به سمت بالا، بازوهای خود را صاف کنید."
msgid "E_D_BENCH_PRESS_CABLE_EXEC_TWO"
msgstr "نفس خود را داخل کشیده و دستگیره‌ها را به آرامی به موقعیت شروع بازگردانید."
msgid "E_D_BENCH_PRESS_CABLE_TIP_ONE"
msgstr "آرنج‌ها را در بالای حرکت قفل نکنید."
msgid "E_D_BENCH_PRESS_CABLE_TIP_TWO"
msgstr "در طول تمرین، عضلات مرکزی (Core) را منقبض نگه دارید."
msgid "E_D_BENT_OVER_BARBELL_ROW_TITLE"
msgstr "Bent Over Barbell Row"
msgid "E_D_BENT_OVER_BARBELL_ROW_FOCUS_AREA"
msgstr "پشت (Back)، دوسر بازو (Biceps)"
msgid "E_D_BENT_OVER_BARBELL_ROW_EQUIPMENT"
msgstr "هالتر"
msgid "E_D_BENT_OVER_BARBELL_ROW_PREP_ONE"
